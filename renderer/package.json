    "render:chapter7": "remotion render WenyanNarration-Chapter7 out/chapter7.mp4",
    "render:chapter8": "remotion render WenyanNarration-Chapter8 out/chapter8.mp4",
    "render:chapter9": "remotion render WenyanNarration-Chapter9 out/chapter9.mp4",
