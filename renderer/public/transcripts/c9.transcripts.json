    "source": "曰四。"
  },
  "c9-s254": {
    "source": "曰五。"
  },
  "c9-s255": {
    "source": "名之曰「勾例」"
  },
  "c9-s256": {
    "source": "曰「股例」"
  },
  "c9-s257": {
    "source": "曰「弦例」。"
  },
  "c9-s258": {
    "source": "\n\n吾有一數。"
  },
  "c9-s259": {
    "source": "曰六。"
  },
  "c9-s260": {
    "source": "名之曰「唯吾知之」。"
  },
  "c9-s261": {
    "source": "注曰『凡以「吾有」言變數者。他書不得引之』"
  },
  "c9-s262": {
    "source": "是書既成。"
  },
  "c9-s263": {
    "source": "乃另作一程式。"
  },
  "c9-s264": {
    "source": "引而驗之。"
  },
  "c9-s265": {
    "source": "吾嘗觀『勾股算經』之書。"
  },
  "c9-s266": {
    "source": "方悟「勾股求弦」「勾弦求股」「勾例」「股例」之義。"
  },
  "c9-s267": {
    "source": "\n\n施「勾股求弦」於「勾例」「股例」。"
  },
  "c9-s268": {
    "source": "書之。"
  },
  "c9-s269": {
    "source": "\n施「勾弦求股」於五。"
  },
  "c9-s270": {
    "source": "於十三。"
  },
  "c9-s271": {
    "source": "書之。"
  },
  "c9-s272": {
    "source": "著引二道。"
  },
  "c9-s273": {
    "source": "至是略備矣。"
  },
  "c9-s274": {
    "source": "程式既長。"
  },
  "c9-s275": {
    "source": "使盡入一文。"
  },
  "c9-s276": {
    "source": "輒如亂粥也。"
  },
  "c9-s277": {
    "source": "故立大事者。"
  },
  "c9-s278": {
    "source": "多擘其文。"
  },
  "c9-s279": {
    "source": "分作數書。"
  },
  "c9-s280": {
    "source": "而更相引用。"
  },
  "c9-s281": {
    "source": "脈絡既明。"
  },
  "c9-s282": {
    "source": "亦大裨益於檢索也。"
  }
}
