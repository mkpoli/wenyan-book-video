  },
  "c6-s228": {
    "source": "終一章一併書之。"
  },
  "c6-s229": {
    "source": "有數一。"
  },
  "c6-s230": {
    "source": "名之曰「章」。"
  },
  "c6-s231": {
    "source": "\n夫「其所」之長。"
  },
  "c6-s232": {
    "source": "為是其遍。"
  },
  "c6-s233": {
    "source": "\n\t吾有一言。"
  },
  "c6-s234": {
    "source": "曰『我所思兮在』。"
  },
  "c6-s235": {
    "source": "夫「其所」之「章」。"
  },
  "c6-s236": {
    "source": "\n\t吾有一言。"
  },
  "c6-s237": {
    "source": "曰『。欲往從之』。"
