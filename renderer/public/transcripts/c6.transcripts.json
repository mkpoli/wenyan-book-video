  "c6-s390": {
    "source": "或出於其圖之界。"
  },
  "c6-s391": {
    "source": "遂以求餘法。"
  },
  "c6-s392": {
    "source": "令出上者下。"
  },
  "c6-s393": {
    "source": "出下者上。"
  },
  "c6-s394": {
    "source": "出左者右。"
  },
  "c6-s395": {
    "source": "出右者左。"
  },
  "c6-s396": {
    "source": "是所謂上下對易。"
  },
  "c6-s397": {
    "source": "左右相更者也。"
  },
  "c6-s398": {
    "source": "乃得「縱」「橫」。"
  },
  "c6-s399": {
    "source": "於是填以「數」。"
  },
