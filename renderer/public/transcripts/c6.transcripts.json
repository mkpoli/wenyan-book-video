  "c6-s244": {
    "source": "夫「所沾」之「章」。"
  },
  "c6-s245": {
    "source": "\n\t吾有一言。"
  },
  "c6-s246": {
    "source": "曰『。美人贈我』。"
  },
  "c6-s247": {
    "source": "夫「所贈」之「章」。"
  },
  "c6-s248": {
    "source": "\n\t吾有一言。"
  },
  "c6-s249": {
    "source": "曰『。何以報之』。"
  },
  "c6-s250": {
    "source": "夫「所報」之「章」。"
  },
  "c6-s251": {
    "source": "\n\t吾有一言。"
  },
  "c6-s252": {
    "source": "曰『。路遠莫致倚』。"
  },
  "c6-s253": {
    "source": "夫「所感」之「章」。"
  },
  "c6-s254": {
    "source": "\n\t吾有一言。"
  },
  "c6-s255": {
    "source": "曰『。何爲懷憂心煩』。"
  },
  "c6-s256": {
    "source": "夫「所傷」之「章」。"
  },
  "c6-s257": {
    "source": "\n\t書之。"
  },
  "c6-s258": {
    "source": "\n\t加「章」以一。"
  },
  "c6-s259": {
    "source": "昔之「章」者。"
  },
  "c6-s260": {
    "source": "今其是矣。"
  },
  "c6-s261": {
    "source": "\n云云。"
  },
  "c6-s262": {
    "source": "乃得原詩如是。"
  },
  "c6-s263": {
    "source": "我所思兮在太山。"
  },
  "c6-s264": {
    "source": "欲往從之樑父艱。"
  },
  "c6-s265": {
    "source": "側身東望涕沾翰。"
  },
  "c6-s266": {
    "source": "美人贈我金錯刀。"
  },
  "c6-s267": {
    "source": "何以報之英瓊瑤。"
  },
  "c6-s268": {
    "source": "路遠莫致倚逍遙。"
  },
  "c6-s269": {
    "source": "何爲懷憂心煩勞\n我所思兮在桂林。"
  },
  "c6-s270": {
    "source": "欲往從之湘水深。"
  },
  "c6-s271": {
    "source": "側身南望涕沾襟。"
  },
  "c6-s272": {
    "source": "美人贈我琴琅玕。"
  },
  "c6-s273": {
    "source": "何以報之雙玉盤。"
  },
  "c6-s274": {
    "source": "路遠莫致倚惆悵。"
  },
  "c6-s275": {
    "source": "何爲懷憂心煩傷\n我所思兮在漢陽。"
  },
  "c6-s276": {
    "source": "欲往從之隴阪長。"
  },
  "c6-s277": {
    "source": "側身西望涕沾裳。"
  },
  "c6-s278": {
    "source": "美人贈我貂襜褕。"
  },
  "c6-s279": {
    "source": "何以報之明月珠。"
  },
  "c6-s280": {
    "source": "路遠莫致倚踟躕。"
  },
  "c6-s281": {
    "source": "何爲懷憂心煩紆\n我所思兮在雁門。"
  },
  "c6-s282": {
    "source": "欲往從之雪雰雰。"
  },
  "c6-s283": {
    "source": "側身北望涕沾巾。"
  },
  "c6-s284": {
    "source": "美人贈我錦繡段。"
  },
  "c6-s285": {
    "source": "何以報之青玉案。"
  },
  "c6-s286": {
    "source": "路遠莫致倚增嘆。"
  },
  "c6-s287": {
    "source": "何爲懷憂心煩惋"
  },
  "c6-s288": {
    "source": "其詩晉人傅玄。"
  },
  "c6-s289": {
    "source": "宋人張載。"
  },
  "c6-s290": {
    "source": "近人魯迅皆嘗為擬作。"
  },
  "c6-s291": {
    "source": "請吾子擇己所好。"
  },
  "c6-s292": {
    "source": "試易前文諸列以得其詩。"
