  },
  "c6-s413": {
    "source": "減其以一。"
  },
  "c6-s414": {
    "source": "除其以「廣」。"
  },
  "c6-s415": {
    "source": "名之曰「掠」。"
  },
  "c6-s416": {
    "source": "\n\t加「半」於「磔」。"
  },
  "c6-s417": {
    "source": "減其以「掠」。"
  },
  "c6-s418": {
    "source": "名之曰「勒」。"
  },
  "c6-s419": {
    "source": "\n\t減「半」於「磔」。"
  },
  "c6-s420": {
    "source": "加其以「掠」。"
  },
  "c6-s421": {
    "source": "名之曰「努」。"
  },
  "c6-s422": {
    "source": "\n\t加「勒」以「廣」。"
  },
  "c6-s423": {
    "source": "除其以「廣」。"
  },
  "c6-s424": {
    "source": "所餘幾何。"
  },
  "c6-s425": {
    "source": "加其以一。"
  },
  "c6-s426": {
    "source": "名之曰「橫」。"
  },
  "c6-s427": {
    "source": "\n\t加「努」以「廣」。"
  },
  "c6-s428": {
    "source": "除其以「廣」。"
  },
  "c6-s429": {
    "source": "所餘幾何。"
  },
  "c6-s430": {
    "source": "加其以一。"
  },
  "c6-s431": {
    "source": "名之曰「縱」。"
  },
  "c6-s432": {
    "source": "\n\t夫「縱橫圖」之「縱」。"
  },
  "c6-s433": {
    "source": "名之曰「行」。"
  },
  "c6-s434": {
    "source": "\n\t昔之「行」之「橫」者。"
  },
  "c6-s435": {
    "source": "今「數」是矣。"
  },
  "c6-s436": {
    "source": "\n加「數」以一。"
  },
  "c6-s437": {
    "source": "昔之「數」者。"
  },
  "c6-s438": {
    "source": "今其是矣。"
  },
  "c6-s439": {
    "source": "云云。"
  },
  "c6-s440": {
    "source": "\n\n凡「縱橫圖」中之「行」。"
  },
  "c6-s441": {
    "source": "夫「行」。"
  },
  "c6-s442": {
    "source": "書之也。"
  },
  "c6-s443": {
    "source": "乃得九九圖如是。"
  },
  "c6-s444": {
    "source": "其與楊氏書中所載不同者。"
  },
  "c6-s445": {
    "source": "蓋作其圖之法甚眾。"
  },
  "c6-s446": {
    "source": "所得亦各有異。"
  },
  "c6-s447": {
    "source": "然其之為縱橫圖者一也。"
  },
  "c6-s448": {
    "source": "一者何耶。"
  },
  "c6-s449": {
    "source": "縱橫斜皆三百六十九也。"
  },
  "c6-s450": {
    "source": "或問曰。"
  },
  "c6-s451": {
    "source": "何以知之。"
  },
  "c6-s452": {
    "source": "曰。"
  },
  "c6-s453": {
    "source": "請吾子試作一術。"
  },
  "c6-s454": {
    "source": "循環遍歷其圖。"
  },
  "c6-s455": {
    "source": "以驗吾言之不謬也。"
  },
