  },
  "c6-s200": {
    "source": "\n充「所感」以『逍遙』以『惆悵』以『踟躕』以『增嘆』。"
  },
  "c6-s201": {
    "source": "\n充「所傷」以『勞』以『傷』以『紆』以『惋』。"
  },
  "c6-s202": {
    "source": "未示其詩。"
  },
  "c6-s203": {
    "source": "請先以一觀諸列。"
  },
  "c6-s204": {
    "source": "`凡「列」中之「元」`者。"
  },
  "c6-s205": {
    "source": "亦循環也。"
  },
  "c6-s206": {
    "source": "是句見於前章。"
  },
  "c6-s207": {
    "source": "斯人所居。"
  },
  "c6-s208": {
    "source": "斯人所遺。"
  },
  "c6-s209": {
    "source": "藉此以列之。"
  },
  "c6-s210": {
    "source": "凡「其所」中之「地」。"
  },
  "c6-s211": {
    "source": "\n\t夫「地」。"
  },
  "c6-s212": {
    "source": "書之。"
  },
  "c6-s213": {
    "source": "\n云云。"
  },
  "c6-s214": {
    "source": "\n吾有一言。"
  },
  "c6-s215": {
    "source": "曰『此皆有所思之地也』。"
  },
  "c6-s216": {
    "source": "書之。"
  },
  "c6-s217": {
    "source": "\n\n凡「所贈」中之「寶」。"
  },
  "c6-s218": {
    "source": "\n\t夫「寶」。"
  },
  "c6-s219": {
    "source": "書之。"
  },
  "c6-s220": {
    "source": "\n云云。"
  },
  "c6-s221": {
    "source": "\n吾有一言。"
  },
  "c6-s222": {
    "source": "曰『此皆美人之所贈也』。"
  },
  "c6-s223": {
    "source": "書之。"
  },
  "c6-s224": {
    "source": "乃以左書張子之詩。"
  },
  "c6-s225": {
    "source": "其循環之體。"
  },
  "c6-s226": {
    "source": "或言復沓。"
  },
  "c6-s227": {
