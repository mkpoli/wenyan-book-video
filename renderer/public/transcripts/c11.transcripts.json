{
  "c11-s1": {
    "source": "夫上蔡臨市而嘆黃。"
  },
  "c11-s2": {
    "source": "杜陵望雲而感蒼。"
  },
  "c11-s3": {
    "source": "括母劾子竟免於族。"
  },
  "c11-s4": {
    "source": "俶父效之猶見於誅。"
  },
  "c11-s5": {
    "source": "宣子以立瑤亡智。"
  },
  "c11-s6": {
    "source": "輔果改氏而逭之。"
  },
  "c11-s7": {
    "source": "北海以譏曹取死。"
  },
  "c11-s8": {
    "source": "二子琢釘而待之。"
  },
  "c11-s9": {
    "source": "天道幽遠以降禍。"
  },
  "c11-s10": {
    "source": "或速或遲。"
  },
  "c11-s11": {
    "source": "世人處之乃顯志。"
  },
  "c11-s12": {
    "source": "有得有失。"
  },
  "c11-s13": {
    "source": "又麋子仲婦人之私。"
  },
  "c11-s14": {
    "source": "火燒其室。"
  },
  "c11-s15": {
    "source": "諸葛恪腥狗之讖。"
  },
  "c11-s16": {
    "source": "兵加其身。"
  },
  "c11-s17": {
    "source": "眾君子謀社宮以亡曹。"
  },
  "c11-s18": {
    "source": "諸小人戲池陽而覆莽。"
  },
  "c11-s19": {
    "source": "永嘉狗語而天下飢。"
  },
  "c11-s20": {
    "source": "太安牛言而天下亂。"
  },
  "c11-s21": {
    "source": "吾人既不得其徵。"
  },
  "c11-s22": {
    "source": "唯綢繆於未雨。"
  },
  "c11-s23": {
    "source": "徙薪於未焚。"
  },
  "c11-s24": {
    "source": "補羊牢於未亡。"
  },
  "c11-s25": {
    "source": "避南牆於未撞。"
  },
  "c11-s26": {
    "source": "故韓非子曰。"
  },
  "c11-s27": {
    "source": "夫事之禍福亦有腠理之地。"
  },
  "c11-s28": {
    "source": "聖人蚤從事焉。"
  },
  "c11-s29": {
    "source": "非獨世事然。"
  },
  "c11-s30": {
    "source": "程式亦然。"
  },
  "c11-s31": {
    "source": "夫程式之運行也。"
  },
  "c11-s32": {
    "source": "或以作者之昏昏。"
  },
  "c11-s33": {
    "source": "禍根為埋。"
  },
  "c11-s34": {
    "source": "或以機器之蹇蹇。"
  },
  "c11-s35": {
    "source": "禍起於內。"
  },
  "c11-s36": {
    "source": "或以用者之渾渾。"
  },
  "c11-s37": {
    "source": "禍起於外。"
  },
  "c11-s38": {
    "source": "或以時命之茫茫。"
  },
  "c11-s39": {
    "source": "禍起於不測。"
  },
  "c11-s40": {
    "source": "既起焉。"
  },
  "c11-s41": {
    "source": "輕者貽誤一時。"
  },
  "c11-s42": {
    "source": "重者民業為廢。"
  },
  "c11-s43": {
    "source": "舊傳。"
  },
  "c11-s44": {
    "source": "初。"
  },
  "c11-s45": {
    "source": "編程草創。"
  },
  "c11-s46": {
    "source": "有前輩程式恆謬者。"
  },
  "c11-s47": {
    "source": "啟機器乃得一蛾。"
  },
  "c11-s48": {
    "source": "方知其由。"
  },
  "c11-s49": {
    "source": "恨不已。"
  },
  "c11-s50": {
    "source": "遂效王莽頭故事。"
  },
  "c11-s51": {
    "source": "以膠封之於紙。"
  },
  "c11-s52": {
    "source": "其屍至今猶傳也。"
  },
  "c11-s53": {
    "source": "程式克禍之法。"
  },
  "c11-s54": {
    "source": "凡成敗不定有取禍之嫌者。"
  },
  "c11-s55": {
    "source": "皆告其姑妄於前。"
  },
  "c11-s56": {
    "source": "料其因由於後。"
  },
  "c11-s57": {
    "source": "乃一一列其解法。"
  },
  "c11-s58": {
    "source": "以為補救。"
  },
  "c11-s59": {
    "source": "多見之禍有四。"
  },
  "c11-s60": {
    "source": "曰虛指。"
  },
  "c11-s61": {
    "source": "曰文法。"
  },
  "c11-s62": {
    "source": "曰異類。"
  },
  "c11-s63": {
    "source": "曰逾界。"
  },
  "c11-s64": {
    "source": "今為是例。"
  },
  "c11-s65": {
    "source": "並書四禍之由。"
  },
  "c11-s66": {
    "source": "姑妄行此。"
  },
  "c11-s67": {
    "source": "\n\t注曰。"
  },
  "c11-s68": {
    "source": "『行險著於是。』"
  },
  "c11-s69": {
    "source": "\n\n如事不諧。"
  },
  "c11-s70": {
    "source": "豈『虛指』之禍歟。"
  },
  "c11-s71": {
    "source": "\n\t吾有一言。"
  },
  "c11-s72": {
    "source": "曰『本無此物。奈何用之』。"
  },
  "c11-s73": {
    "source": "書之。"
  },
  "c11-s74": {
    "source": "\n\n豈『文法』之禍歟。"
  },
  "c11-s75": {
    "source": "\n\t吾有一言。"
  },
  "c11-s76": {
    "source": "曰『不合文法。不通之甚』。"
  },
  "c11-s77": {
    "source": "書之。"
  },
  "c11-s78": {
    "source": "\n\n豈『異類』之禍歟。"
  },
  "c11-s79": {
    "source": "\n\t吾有一言。"
  },
  "c11-s80": {
    "source": "曰『物各其類。豈可混同』。"
  },
  "c11-s81": {
    "source": "書之。"
  },
  "c11-s82": {
    "source": "\n\t\n豈『逾界』之禍歟。"
  },
  "c11-s83": {
    "source": "\n\t吾有一言。"
  },
  "c11-s84": {
    "source": "曰『祭不越望。俎不代庖』。"
  },
  "c11-s85": {
    "source": "書之。"
  },
  "c11-s86": {
    "source": "\n\n不知何禍歟。"
  },
  "c11-s87": {
    "source": "\n\t吾有一言。"
  },
  "c11-s88": {
    "source": "曰『嗚呼哀哉。伏維尚飨』。"
  },
  "c11-s89": {
    "source": "書之。"
  },
  "c11-s90": {
    "source": "\n乃作罷。"
  },
  "c11-s91": {
    "source": "若姑妄之行未便致禍者。"
  },
  "c11-s92": {
    "source": "則解法不行。"
  },
  "c11-s93": {
    "source": "既致。"
  },
  "c11-s94": {
    "source": "則立視其解法。"
  },
  "c11-s95": {
    "source": "合者輒為之。"
  },
  "c11-s96": {
    "source": "其餘更不復行。"
  },
  "c11-s97": {
    "source": "直繼以「乃作罷」語之後也。"
  },
  "c11-s98": {
    "source": "於諸解法之末。"
  },
  "c11-s99": {
    "source": "或加「不知何禍歟」之語。"
  },
  "c11-s100": {
    "source": "以備其未料者。"
  },
  "c11-s101": {
    "source": "微是。"
  },
  "c11-s102": {
    "source": "終不得解法。"
  },
  "c11-s103": {
    "source": "乃崩潰焉。"
  },
  "c11-s104": {
    "source": "復有報錯之法。"
  },
  "c11-s105": {
    "source": "所以自定一禍而發之也。"
  },
  "c11-s106": {
    "source": "或問曰。"
  },
  "c11-s107": {
    "source": "誕矣。"
  },
  "c11-s108": {
    "source": "無禍則當以手加額而慶焉。"
  },
  "c11-s109": {
    "source": "奈何故為禍乎。"
  },
  "c11-s110": {
    "source": "曰。"
  },
  "c11-s111": {
    "source": "其一者。"
  },
  "c11-s112": {
    "source": "不濟之兆既見。"
  },
  "c11-s113": {
    "source": "與其坐待其潰。"
  },
  "c11-s114": {
    "source": "不若於未崩之時先行告急。"
  },
  "c11-s115": {
    "source": "更以克禍之法為補救也。"
  },
  "c11-s116": {
    "source": "其二者。"
  },
  "c11-s117": {
    "source": "夫程式之作也。"
  },
  "c11-s118": {
    "source": "多集眾力。"
  },
  "c11-s119": {
    "source": "或各為一部。"
  },
  "c11-s120": {
    "source": "或互為徵引。"
  },
  "c11-s121": {
    "source": "故告之者。"
  },
  "c11-s122": {
    "source": "處之者。"
  },
  "c11-s123": {
    "source": "未必同一人也。"
  },
  "c11-s124": {
    "source": "至於用之者。"
  },
  "c11-s125": {
    "source": "天下之人也。"
  },
  "c11-s126": {
    "source": "故曰。"
  },
  "c11-s127": {
    "source": "吾之報之者。"
  },
  "c11-s128": {
    "source": "所以令彼知其不行之由也。"
  },
  "c11-s129": {
    "source": "至於解者。"
  },
  "c11-s130": {
    "source": "或彼之責也。"
  },
  "c11-s131": {
    "source": "報錯之法如是。"
  },
  "c11-s132": {
    "source": "嗚呼。"
  },
  "c11-s133": {
    "source": "『滅頂』之禍。"
  },
  "c11-s134": {
    "source": "夫「滅頂」者。"
  },
  "c11-s135": {
    "source": "所以名禍也。"
  },
  "c11-s136": {
    "source": "視禍之因由而後定也。"
  },
  "c11-s137": {
    "source": "亦得以「虛指」「文法」「異類」「逾界」名之。"
  },
  "c11-s138": {
    "source": "以合舊製。"
  },
  "c11-s139": {
    "source": "既報。"
  },
  "c11-s140": {
    "source": "解法之作同於向者。"
  },
  "c11-s141": {
    "source": "姑妄行此。"
  },
  "c11-s142": {
    "source": "\n\t嗚呼。"
  },
  "c11-s143": {
    "source": "『滅頂』之禍。"
  },
  "c11-s144": {
    "source": "\n如事不諧。"
  },
  "c11-s145": {
    "source": "豈『滅頂』之禍歟。"
  },
  "c11-s146": {
    "source": "\n\t吾有一言。"
  },
  "c11-s147": {
    "source": "曰『嗚呼哀哉。伏維尚飨』。"
  },
  "c11-s148": {
    "source": "書之。"
  },
  "c11-s149": {
    "source": "\n乃作罷。"
  },
  "c11-s150": {
    "source": "又或加一言於禍後。"
  },
  "c11-s151": {
    "source": "以明細得禍之本來。"
  },
  "c11-s152": {
    "source": "蓋禍之名或一。"
  },
  "c11-s153": {
    "source": "而取之之由非一。"
  },
  "c11-s154": {
    "source": "故以是釋之。"
  },
  "c11-s155": {
    "source": "曰禍之「文」。"
  },
  "c11-s156": {
    "source": "例曰。"
  },
  "c11-s157": {
    "source": "嗚呼。"
  },
  "c11-s158": {
    "source": "『無關』之禍。"
  },
  "c11-s159": {
    "source": "曰『事不關心。關心者亂』。"
  },
  "c11-s160": {
    "source": "\n嗚呼。"
  },
  "c11-s161": {
    "source": "『自取』之禍。"
  },
  "c11-s162": {
    "source": "曰『招禍取咎。無不自己』。"
  },
  "c11-s163": {
    "source": "或問曰。"
  },
  "c11-s164": {
    "source": "於解法中何由而得之耶。"
  },
  "c11-s165": {
    "source": "曰。"
  },
  "c11-s166": {
    "source": "可蓄其禍之於一物而名之。"
  },
  "c11-s167": {
    "source": "如左。"
  },
  "c11-s168": {
    "source": "其「名」「文」既明。"
  },
  "c11-s169": {
    "source": "至於其「蹤」者。"
  },
  "c11-s170": {
    "source": "所以敘得禍之所。"
  },
  "c11-s171": {
    "source": "以為檢索之便也。"
  },
  "c11-s172": {
    "source": "姑妄行此。"
  },
  "c11-s173": {
    "source": "\n\t嗚呼。"
  },
  "c11-s174": {
    "source": "『無關』之禍。"
  },
  "c11-s175": {
    "source": "曰『事不關心。關心者亂』。"
  },
  "c11-s176": {
    "source": "\n如事不諧。"
  },
  "c11-s177": {
    "source": "豈『無關』之禍歟。"
  },
  "c11-s178": {
    "source": "名之曰「禍」。"
  },
  "c11-s179": {
    "source": "\n\t夫「禍」之『名』。"
  },
  "c11-s180": {
    "source": "書之。"
  },
  "c11-s181": {
    "source": "\n\t夫「禍」之『文』。"
  },
  "c11-s182": {
    "source": "書之。"
  },
  "c11-s183": {
    "source": "\n\t夫「禍」之『蹤』。"
  },
  "c11-s184": {
    "source": "書之。"
  },
  "c11-s185": {
    "source": "\n不知何禍歟。"
  },
  "c11-s186": {
    "source": "名之曰「奇禍」。"
  },
  "c11-s187": {
    "source": "\n\t夫「奇禍」之『名』。"
  },
  "c11-s188": {
    "source": "書之。"
  },
  "c11-s189": {
    "source": "\n乃作罷。"
  },
  "c11-s190": {
    "source": "問。"
  },
  "c11-s191": {
    "source": "除一數以零。"
  },
  "c11-s192": {
    "source": "其商幾何。"
  },
  "c11-s193": {
    "source": "今以之為例。"
  },
  "c11-s194": {
    "source": "吾有一術。"
  },
  "c11-s195": {
    "source": "名之曰「除」。"
  },
  "c11-s196": {
    "source": "欲行是術。"
  },
  "c11-s197": {
    "source": "必先得二數。"
  },
  "c11-s198": {
    "source": "曰「甲」。"
  },
  "c11-s199": {
    "source": "曰「乙」。"
  },
  "c11-s200": {
    "source": "乃行是術曰。"
  },
  "c11-s201": {
    "source": "\n\t若「乙」等於零者。"
  },
  "c11-s202": {
    "source": "\n\t\t嗚呼。"
  },
  "c11-s203": {
    "source": "『零除』之禍。"
  },
  "c11-s204": {
    "source": "曰『除數不得為零』。"
  },
  "c11-s205": {
    "source": "\n\t云云。"
  },
  "c11-s206": {
    "source": "\n\t除「甲」以「乙」。"
  },
  "c11-s207": {
    "source": "乃得矣。"
  },
  "c11-s208": {
    "source": "\n是謂「除」之術也。"
  },
  "c11-s209": {
    "source": "\n\n施「除」於二。"
  },
  "c11-s210": {
    "source": "於三。"
  },
  "c11-s211": {
    "source": "書之。"
  },
  "c11-s212": {
    "source": "\n施「除」於四。"
  },
  "c11-s213": {
    "source": "於零。"
  },
  "c11-s214": {
    "source": "書之。"
  },
  "c11-s215": {
    "source": "或曰。"
  },
  "c11-s216": {
    "source": "零除誠為非。"
  },
  "c11-s217": {
    "source": "然以其漸近無窮。"
  },
  "c11-s218": {
    "source": "不必即為禍。"
  },
  "c11-s219": {
    "source": "今以克禍之法。"
  },
  "c11-s220": {
    "source": "稍加寬恕。"
  },
  "c11-s221": {
    "source": "取至大之數而聊為出脫。"
  },
  "c11-s222": {
    "source": "至於零除零者。"
  },
  "c11-s223": {
    "source": "則實無可解。"
  },
  "c11-s224": {
    "source": "乃於解法之體更報一錯。"
  },
  "c11-s225": {
    "source": "不令姑息也。"
  },
  "c11-s226": {
    "source": "吾有一術。"
  },
  "c11-s227": {
    "source": "名之曰「除乙」。"
  },
  "c11-s228": {
    "source": "欲行是術。"
  },
  "c11-s229": {
    "source": "必先得二數。"
  },
  "c11-s230": {
    "source": "曰「甲」。"
  },
  "c11-s231": {
    "source": "曰「乙」。"
  },
  "c11-s232": {
    "source": "乃行是術曰。"
  },
  "c11-s233": {
    "source": "\n\t姑妄行此。"
  },
  "c11-s234": {
    "source": "\n\t\t施「除」於「甲」。"
  },
  "c11-s235": {
    "source": "於「乙」。"
  },
  "c11-s236": {
    "source": "乃得矣。"
  },
  "c11-s237": {
    "source": "\n\t如事不諧。"
  },
  "c11-s238": {
    "source": "豈『零除』之禍歟。"
  },
  "c11-s239": {
    "source": "\n\t\t若「甲」大於零者。"
  },
  "c11-s240": {
    "source": "\n\t\t\t乃得一載。"
  },
  "c11-s241": {
    "source": "\n\t\t或若「甲」小於零者。"
  },
  "c11-s242": {
    "source": "\n\t\t\t乃得負一載。"
  },
  "c11-s243": {
    "source": "\n\t\t若非。"
  },
  "c11-s244": {
    "source": "\n\t\t\t嗚呼。"
  },
  "c11-s245": {
    "source": "『零除零』之禍。"
  },
  "c11-s246": {
    "source": "曰『二數皆零。無可救也』。"
  },
  "c11-s247": {
    "source": "\n\t\t云云。"
  },
  "c11-s248": {
    "source": "\n\t乃作罷。"
  },
  "c11-s249": {
    "source": "\n是謂「除乙」之術也。"
  },
  "c11-s250": {
    "source": "或又曰。"
  },
  "c11-s251": {
    "source": "零除零誠為非。"
  },
  "c11-s252": {
    "source": "然今實不欲致禍。"
  },
  "c11-s253": {
    "source": "強令得零可乎。"
  },
  "c11-s254": {
    "source": "曰。"
  },
  "c11-s255": {
    "source": "吾有一術。"
  },
  "c11-s256": {
    "source": "名之曰「除丙」。"
  },
  "c11-s257": {
    "source": "欲行是術。"
  },
  "c11-s258": {
    "source": "必先得二數。"
  },
  "c11-s259": {
    "source": "曰「甲」。"
  },
  "c11-s260": {
    "source": "曰「乙」。"
  },
  "c11-s261": {
    "source": "乃行是術曰。"
  },
  "c11-s262": {
    "source": "\n\t姑妄行此。"
  },
  "c11-s263": {
    "source": "\n\t\t施「除乙」於「甲」。"
  },
  "c11-s264": {
    "source": "於「乙」。"
  },
  "c11-s265": {
    "source": "乃得矣。"
  },
  "c11-s266": {
    "source": "\n\t如事不諧。"
  },
  "c11-s267": {
    "source": "豈『零除零』之禍歟。"
  },
  "c11-s268": {
    "source": "\n\t\t乃得零。"
  },
  "c11-s269": {
    "source": "注曰『氣殺算學先生也』。"
  },
  "c11-s270": {
    "source": "\n\t乃作罷。"
  },
  "c11-s271": {
    "source": "\n是謂「除丙」之術也。"
  },
  "c11-s272": {
    "source": "又前章有莊子棰術。"
  },
  "c11-s273": {
    "source": "今以報錯法。"
  },
  "c11-s274": {
    "source": "易之如是。"
  },
  "c11-s275": {
    "source": "以禁不宜之參。"
  },
  "c11-s276": {
    "source": "吾有一術。"
  },
  "c11-s277": {
    "source": "名之曰「量棰」。"
  },
  "c11-s278": {
    "source": "欲行是術。"
  },
  "c11-s279": {
    "source": "必先得二數。"
  },
  "c11-s280": {
    "source": "曰「今長」。"
  },
  "c11-s281": {
    "source": "曰「餘日」。"
  },
  "c11-s282": {
    "source": "乃行是術曰。"
  },
  "c11-s283": {
    "source": "\n\t若「今長」不大於零者。"
  },
  "c11-s284": {
    "source": "嗚呼。"
  },
  "c11-s285": {
    "source": "『參數』之禍。"
  },
  "c11-s286": {
    "source": "曰『萬世不竭。必當為正』也。"
  },
  "c11-s287": {
    "source": "\n\t若「餘日」小於零者。"
  },
  "c11-s288": {
    "source": "　嗚呼。"
  },
  "c11-s289": {
    "source": "『參數』之禍。"
  },
  "c11-s290": {
    "source": "曰『逝者如斯。豈得逆耶』也。"
  },
  "c11-s291": {
    "source": "\n\t\n\t若「餘日」等於零者。"
  },
  "c11-s292": {
    "source": "乃得「今長」也。"
  },
  "c11-s293": {
    "source": "\n\t除「今長」以二。"
  },
  "c11-s294": {
    "source": "昔之「今長」者。"
  },
  "c11-s295": {
    "source": "今其是矣。"
  },
  "c11-s296": {
    "source": "\n\t減「餘日」以一。"
  },
  "c11-s297": {
    "source": "昔之「餘日」者。"
  },
  "c11-s298": {
    "source": "今其是矣。"
  },
  "c11-s299": {
    "source": "\n\t施「量棰」於「今長」於「餘日」。"
  },
  "c11-s300": {
    "source": "乃得矣。"
  },
  "c11-s301": {
    "source": "\n是謂「量棰」之術也。"
  },
  "c11-s302": {
    "source": "凡一術未行。"
  },
  "c11-s303": {
    "source": "先驗其參數如是者。"
  },
  "c11-s304": {
    "source": "或號之曰「斷言」。"
  },
  "c11-s305": {
    "source": "「斷言」者。"
  },
  "c11-s306": {
    "source": "如繞城之池。"
  },
  "c11-s307": {
    "source": "護齒之唇。"
  },
  "c11-s308": {
    "source": "省己之良方。"
  },
  "c11-s309": {
    "source": "警彼之佳策也。"
  },
  "c11-s310": {
    "source": "欲有所為者多為是。"
  },
  "c11-s311": {
    "source": "克禍之道。"
  },
  "c11-s312": {
    "source": "至是略備矣。"
  },
  "c11-s313": {
    "source": "或曰。"
  },
  "c11-s314": {
    "source": "妙哉其道。"
  },
  "c11-s315": {
    "source": "吾之程式。"
  },
  "c11-s316": {
    "source": "將以「姑妄行此」起之。"
  },
  "c11-s317": {
    "source": "以「不知何禍歟」結之。"
  },
  "c11-s318": {
    "source": "千謬百錯盡為所苞。"
  },
  "c11-s319": {
    "source": "永不得禍矣。"
  },
  "c11-s320": {
    "source": "豈不美哉。"
  },
  "c11-s321": {
    "source": "曰。"
  },
  "c11-s322": {
    "source": "殆矣。"
  },
  "c11-s323": {
    "source": "陋之甚。"
  },
  "c11-s324": {
    "source": "小禍不與聞。"
  },
  "c11-s325": {
    "source": "大禍不遠矣。"
  },
  "c11-s326": {
    "source": "吾子當其慎之。"
  }
}
