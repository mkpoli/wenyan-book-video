  "c7-s246": {
    "source": "既無少益於程式之運行。"
  },
  "c7-s247": {
    "source": "吾法吾自知之。"
  },
  "c7-s248": {
    "source": "奚注為。"
  },
  "c7-s249": {
    "source": "曰。"
  },
  "c7-s250": {
    "source": "所以資同事者後人之觀也。"
  },
  "c7-s251": {
    "source": "蓋程式之欲大有所為者。"
  },
  "c7-s252": {
    "source": "多非一時之力。"
  },
  "c7-s253": {
    "source": "一人之功。"
  },
  "c7-s254": {
    "source": "他人得觀其注。"
  },
  "c7-s255": {
    "source": "立明其義。"
  },
  "c7-s256": {
    "source": "刪補增修。"
  },
  "c7-s257": {
    "source": "得於心而應於手。"
  },
  "c7-s258": {
    "source": "余少時。"
  },
  "c7-s259": {
    "source": "下筆了不容思。"
  },
  "c7-s260": {
    "source": "乃以批注為累。"
  },
  "c7-s261": {
    "source": "灑灑千言。"
  },
  "c7-s262": {
    "source": "卒無一注。"
  },
  "c7-s263": {
    "source": "故至今為時人所病。"
  },
  "c7-s264": {
    "source": "不亦悲夫。"
  },
  "c7-s265": {
    "source": "故聰明君子。"
  },
  "c7-s266": {
    "source": "莫我效焉。"
  },
  "c7-s267": {
    "source": "注有二必注。"
  },
  "c7-s268": {
    "source": "有二不注。"
  },
  "c7-s269": {
    "source": "曰。"
  },
  "c7-s270": {
    "source": "開宗明義者必注"
  },
  "c7-s271": {
    "source": "奇技淫巧者必注"
  },
  "c7-s272": {
    "source": "用意自明者不注"
  },
  "c7-s273": {
    "source": "為注而注者不注"
  },
  "c7-s274": {
    "source": "除是外。"
  },
  "c7-s275": {
    "source": "復有一妙用。"
  },
  "c7-s276": {
    "source": "作程式之時。"
  },
  "c7-s277": {
    "source": "往往調試無已。"
  },
  "c7-s278": {
    "source": "夫置一句乎注釋之內。"
  },
  "c7-s279": {
    "source": "可聊止其運行。"
  },
  "c7-s280": {
    "source": "然或曰程式既成乃當去之。"
  },
  "c7-s281": {
    "source": "蓋以其醜陋云耳。"
  },
  "c7-s282": {
    "source": "注曰。"
  },
  "c7-s283": {
    "source": "『吾有一言。曰『是語處批注之中。故終不得行』。"
  },
  "c7-s284": {
    "source": "書之。"
  },
  "c7-s285": {
    "source": "』"
  },
  "c7-s286": {
    "source": "注釋之方既備。"
  },
  "c7-s287": {
    "source": "後章之範例。"
  },
  "c7-s288": {
    "source": "皆以為剖析。"
  }
}
