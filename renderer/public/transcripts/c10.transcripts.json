  "c10-s66": {
    "source": "昔之「白馬」之『日行里數』者。"
  },
  "c10-s67": {
    "source": "今其是矣。"
  },
  "c10-s68": {
    "source": "\n夫「白馬」之『夜行里數』。"
  },
  "c10-s69": {
    "source": "減其以五百。"
