{
  "c10-s1": {
    "source": "老子云。"
  },
  "c10-s2": {
    "source": "道生一。"
  },
  "c10-s3": {
    "source": "一生二。"
  },
  "c10-s4": {
    "source": "二生三。"
  },
  "c10-s5": {
    "source": "三生萬物。"
  },
  "c10-s6": {
    "source": "列子云。"
  },
  "c10-s7": {
    "source": "凡有貌象聲色者。"
  },
  "c10-s8": {
    "source": "皆物也。"
  },
  "c10-s9": {
    "source": "物之既有此眾端者。"
  },
  "c10-s10": {
    "source": "數言爻列術或不足表。"
