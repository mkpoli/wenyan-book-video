  },
  "c10-s17": {
    "source": "人之年齒者。"
  },
  "c10-s18": {
    "source": "數也。"
  },
  "c10-s19": {
    "source": "人有男女。"
  },
  "c10-s20": {
    "source": "陰陽也。"
  },
  "c10-s21": {
    "source": "或有所蓄。"
  },
  "c10-s22": {
