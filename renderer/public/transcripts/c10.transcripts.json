  "c10-s54": {
    "source": "數曰四。"
  },
  "c10-s55": {
    "source": "\n是謂「白馬」之物也。"
  },
  "c10-s56": {
    "source": "其物既敘。"
  },
  "c10-s57": {
    "source": "復得取其端書如是。"
  },
  "c10-s58": {
    "source": "夫「白馬」之『名』。"
  },
  "c10-s59": {
    "source": "書之。"
  },
  "c10-s60": {
    "source": "\n夫「白馬」之『日行里數』。"
