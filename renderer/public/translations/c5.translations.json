    "source": "愚以為。",
    "translation": ""
  },
  "c5-s199": {
    "source": "凡循環之體冗長者。",
    "translation": ""
  },
  "c5-s200": {
    "source": "預以`乃止是遍`排其特例。",
    "translation": ""
  },
  "c5-s201": {
    "source": "可免於決策重層累然之苦。",
    "translation": ""
  },
  "c5-s202": {
    "source": "人或有不以為然者。",
    "translation": ""
  },
  "c5-s203": {
    "source": "曰。",
    "translation": ""
  },
  "c5-s204": {
    "source": "去止之方既多。",
    "translation": ""
  },
  "c5-s205": {
    "source": "蟄伏深隱。",
    "translation": ""
  },
  "c5-s206": {
    "source": "乃至難以理喻。",
    "translation": ""
  },
  "c5-s207": {
    "source": "是取亂之道也。",
    "translation": ""
  },
  "c5-s208": {
    "source": "是亦理也。",
    "translation": ""
  },
  "c5-s209": {
    "source": "編程之兩難者多如是。",
    "translation": ""
  },
  "c5-s210": {
    "source": "君子可自取所好焉。",
    "translation": ""
  },
  "c5-s211": {
    "source": "循環之術。",
    "translation": ""
  },
  "c5-s212": {
    "source": "至是略備矣。",
    "translation": ""
  },
  "c5-s213": {
    "source": "總其五章所知。",
    "translation": ""
  },
  "c5-s214": {
    "source": "亦頗有可為者。",
    "translation": ""
  },
  "c5-s215": {
    "source": "吾聞之。",
    "translation": ""
  },
  "c5-s216": {
    "source": "夫欲長於編程者。",
    "translation": ""
  },
  "c5-s217": {
    "source": "唯學而立用。",
    "translation": ""
  },
  "c5-s218": {
    "source": "用而不止。",
    "translation": ""
  },
  "c5-s219": {
    "source": "其勝束其手而觀其書百倍。",
    "translation": ""
  },
  "c5-s220": {
    "source": "善哉此言。",
    "translation": ""
  },
  "c5-s221": {
    "source": "學而不思則罔。",
    "translation": ""
  },
  "c5-s222": {
    "source": "吾聞諸仲尼。",
    "translation": ""
  },
  "c5-s223": {
    "source": "請試之。",
    "translation": ""
  },
  "c5-s224": {
