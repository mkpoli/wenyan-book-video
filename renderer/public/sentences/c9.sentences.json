{
  "chapterId": "c9",
  "number": 9,
  "title": "府庫第九",
  "sentences": [
    {
      "id": "c9-s1",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 1,
      "source": "夫人之欲蓄一物。",
      "isCode": false
    },
    {
      "id": "c9-s2",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 2,
      "source": "或諸袖中。",
      "isCode": false
    },
    {
      "id": "c9-s3",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 3,
      "source": "或諸案左。",
      "isCode": false
    },
    {
      "id": "c9-s4",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 4,
      "source": "物其多也。",
      "isCode": false
    },
    {
      "id": "c9-s5",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 5,
      "source": "乃藏諸箱篋。",
      "isCode": false
    },
    {
      "id": "c9-s6",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 6,
      "source": "儲諸籠笥。",
      "isCode": false
    },
    {
      "id": "c9-s7",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 7,
      "source": "篋笥不能盡容。",
      "isCode": false
    },
    {
      "id": "c9-s8",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 8,
      "source": "復載以牛車。",
      "isCode": false
    },
    {
      "id": "c9-s9",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 9,
      "source": "束以高閣。",
      "isCode": false
    },
    {
      "id": "c9-s10",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 10,
      "source": "彼亦不能足者。",
      "isCode": false
    },
    {
      "id": "c9-s11",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 11,
      "source": "乃入山斷木。",
      "isCode": false
    },
    {
      "id": "c9-s12",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 12,
      "source": "文梓楩柟。",
      "isCode": false
    },
    {
      "id": "c9-s13",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 13,
      "source": "巧工施校。",
      "isCode": false
    },
    {
      "id": "c9-s14",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 14,
      "source": "為雕梁畫棟。",
      "isCode": false
    },
    {
      "id": "c9-s15",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 15,
      "source": "樓閣起焉。",
      "isCode": false
    },
    {
      "id": "c9-s16",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 16,
      "source": "蓄一國之物。",
      "isCode": false
    },
    {
      "id": "c9-s17",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 17,
      "source": "乃興冊府。",
      "isCode": false
    },
    {
      "id": "c9-s18",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 18,
      "source": "武庫。",
      "isCode": false
    },
    {
      "id": "c9-s19",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 19,
      "source": "倉廩。",
      "isCode": false
    },
    {
      "id": "c9-s20",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 20,
      "source": "以羅天下之可寶。",
      "isCode": false
    },
    {
      "id": "c9-s21",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 21,
      "source": "今以編程之繁複。",
      "isCode": false
    },
    {
      "id": "c9-s22",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 22,
      "source": "亦設府庫以輯有用之法。",
      "isCode": false
    },
    {
      "id": "c9-s23",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 23,
      "source": "既驗之策。",
      "isCode": false
    },
    {
      "id": "c9-s24",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 24,
      "source": "如書齋然。",
      "isCode": false
    },
    {
      "id": "c9-s25",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 25,
      "source": "如箱篋然。",
      "isCode": false
    },
    {
      "id": "c9-s26",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 26,
      "source": "唯時取之。",
      "isCode": false
    },
    {
      "id": "c9-s27",
      "chapterId": "c9",
      "blockId": "b1",
      "index": 27,
      "source": "以為作者助也。",
      "isCode": false
    },
    {
      "id": "c9-s28",
      "chapterId": "c9",
      "blockId": "b2",
      "index": 28,
      "source": "所藏者。",
      "isCode": false
    },
    {
      "id": "c9-s29",
      "chapterId": "c9",
      "blockId": "b2",
      "index": 29,
      "source": "術之集也。",
      "isCode": false
    },
    {
      "id": "c9-s30",
      "chapterId": "c9",
      "blockId": "b2",
      "index": 30,
      "source": "法古而名之曰書。",
      "isCode": false
    },
    {
      "id": "c9-s31",
      "chapterId": "c9",
      "blockId": "b2",
      "index": 31,
      "source": "今擷數書為例。",
      "isCode": false
    },
    {
      "id": "c9-s32",
      "chapterId": "c9",
      "blockId": "b2",
      "index": 32,
      "source": "計開。",
      "isCode": false
    },
    {
      "id": "c9-s33",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 33,
      "source": "算經者。",
      "isCode": false
    },
    {
      "id": "c9-s34",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 34,
      "source": "所以資算術也。",
      "isCode": false
    },
    {
      "id": "c9-s35",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 35,
      "source": "開方。",
      "isCode": false
    },
    {
      "id": "c9-s36",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 36,
      "source": "勾股。",
      "isCode": false
    },
    {
      "id": "c9-s37",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 37,
      "source": "密率。",
      "isCode": false
    },
    {
      "id": "c9-s38",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 38,
      "source": "此之類也。",
      "isCode": false
    },
    {
      "id": "c9-s39",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 39,
      "source": "列經者。",
      "isCode": false
    },
    {
      "id": "c9-s40",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 40,
      "source": "所以析列也。",
      "isCode": false
    },
    {
      "id": "c9-s41",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 41,
      "source": "篩剔。",
      "isCode": false
    },
    {
      "id": "c9-s42",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 42,
      "source": "合併。",
      "isCode": false
    },
    {
      "id": "c9-s43",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 43,
      "source": "排序。",
      "isCode": false
    },
    {
      "id": "c9-s44",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 44,
      "source": "此之類也。",
      "isCode": false
    },
    {
      "id": "c9-s45",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 45,
      "source": "曆法者。",
      "isCode": false
    },
    {
      "id": "c9-s46",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 46,
      "source": "演算天時也。",
      "isCode": false
    },
    {
      "id": "c9-s47",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 47,
      "source": "求年月日時刻。",
      "isCode": false
    },
    {
      "id": "c9-s48",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 48,
      "source": "此之類也。",
      "isCode": false
    },
    {
      "id": "c9-s49",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 49,
      "source": "畫譜者。",
      "isCode": false
    },
    {
      "id": "c9-s50",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 50,
      "source": "所以為圖畫也。",
      "isCode": false
    },
    {
      "id": "c9-s51",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 51,
      "source": "勾描。",
      "isCode": false
    },
    {
      "id": "c9-s52",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 52,
      "source": "皴法。",
      "isCode": false
    },
    {
      "id": "c9-s53",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 53,
      "source": "設色。",
      "isCode": false
    },
    {
      "id": "c9-s54",
      "chapterId": "c9",
      "blockId": "b3",
      "index": 54,
      "source": "此之類也。",
      "isCode": false
    },
    {
      "id": "c9-s55",
      "chapterId": "c9",
      "blockId": "b4",
      "index": 55,
      "source": "欲得一書一術。",
      "isCode": false
    },
    {
      "id": "c9-s56",
      "chapterId": "c9",
      "blockId": "b4",
      "index": 56,
      "source": "乃引之如是。",
      "isCode": false
    },
    {
      "id": "c9-s57",
      "chapterId": "c9",
      "blockId": "b5",
      "index": 57,
      "source": "吾嘗觀『算經』之書。",
      "isCode": true
    },
    {
      "id": "c9-s58",
      "chapterId": "c9",
      "blockId": "b5",
      "index": 58,
      "source": "方悟「平方根」之義。",
      "isCode": true
    },
    {
      "id": "c9-s59",
      "chapterId": "c9",
      "blockId": "b6",
      "index": 59,
      "source": "既得「平方根」之術。",
      "isCode": false
    },
    {
      "id": "c9-s60",
      "chapterId": "c9",
      "blockId": "b6",
      "index": 60,
      "source": "遂得而用之如是。",
      "isCode": false
    },
    {
      "id": "c9-s61",
      "chapterId": "c9",
      "blockId": "b7",
      "index": 61,
      "source": "施「平方根」於八十一。",
      "isCode": true
    },
    {
      "id": "c9-s62",
      "chapterId": "c9",
      "blockId": "b7",
      "index": 62,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s63",
      "chapterId": "c9",
      "blockId": "b8",
      "index": 63,
      "source": "乃得「九」耳。",
      "isCode": false
    },
    {
      "id": "c9-s64",
      "chapterId": "c9",
      "blockId": "b8",
      "index": 64,
      "source": "欲取非一者。",
      "isCode": false
    },
    {
      "id": "c9-s65",
      "chapterId": "c9",
      "blockId": "b8",
      "index": 65,
      "source": "但魚貫之可也。",
      "isCode": false
    },
    {
      "id": "c9-s66",
      "chapterId": "c9",
      "blockId": "b8",
      "index": 66,
      "source": "例曰。",
      "isCode": false
    },
    {
      "id": "c9-s67",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 67,
      "source": "吾嘗觀『算經』之書。",
      "isCode": true
    },
    {
      "id": "c9-s68",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 68,
      "source": "方悟「圓周率」「勾股求弦」「冪」之義。",
      "isCode": true
    },
    {
      "id": "c9-s69",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 69,
      "source": "\n吾嘗觀『曆法』之書。",
      "isCode": true
    },
    {
      "id": "c9-s70",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 70,
      "source": "方悟「今日何日」之義。",
      "isCode": true
    },
    {
      "id": "c9-s71",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 71,
      "source": "\n\n乘「圓周率」以四。",
      "isCode": true
    },
    {
      "id": "c9-s72",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 72,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s73",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 73,
      "source": "\n\n施「勾股求弦」於三。",
      "isCode": true
    },
    {
      "id": "c9-s74",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 74,
      "source": "於四。",
      "isCode": true
    },
    {
      "id": "c9-s75",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 75,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s76",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 76,
      "source": "\n\n施「冪」於二。",
      "isCode": true
    },
    {
      "id": "c9-s77",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 77,
      "source": "於十。",
      "isCode": true
    },
    {
      "id": "c9-s78",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 78,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s79",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 79,
      "source": "\n\n施「今日何日」。",
      "isCode": true
    },
    {
      "id": "c9-s80",
      "chapterId": "c9",
      "blockId": "b9",
      "index": 80,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s81",
      "chapterId": "c9",
      "blockId": "b10",
      "index": 81,
      "source": "又聊取列經之諸術。",
      "isCode": false
    },
    {
      "id": "c9-s82",
      "chapterId": "c9",
      "blockId": "b10",
      "index": 82,
      "source": "略述其用也。",
      "isCode": false
    },
    {
      "id": "c9-s83",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 83,
      "source": "吾嘗觀『列經』之書。",
      "isCode": true
    },
    {
      "id": "c9-s84",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 84,
      "source": "方悟「遍施」「篩剔」「左併」「右併」「排序」「倒序」「索一」之義。",
      "isCode": true
    },
    {
      "id": "c9-s85",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 85,
      "source": "\n\n吾有一列。",
      "isCode": true
    },
    {
      "id": "c9-s86",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 86,
      "source": "名之曰「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s87",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 87,
      "source": "充「甲」以三。",
      "isCode": true
    },
    {
      "id": "c9-s88",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 88,
      "source": "以二。",
      "isCode": true
    },
    {
      "id": "c9-s89",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 89,
      "source": "以九。",
      "isCode": true
    },
    {
      "id": "c9-s90",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 90,
      "source": "以一。",
      "isCode": true
    },
    {
      "id": "c9-s91",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 91,
      "source": "以六。",
      "isCode": true
    },
    {
      "id": "c9-s92",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 92,
      "source": "以十七。",
      "isCode": true
    },
    {
      "id": "c9-s93",
      "chapterId": "c9",
      "blockId": "b11",
      "index": 93,
      "source": "以四十二。",
      "isCode": true
    },
    {
      "id": "c9-s94",
      "chapterId": "c9",
      "blockId": "b12",
      "index": 94,
      "source": "遍施者。",
      "isCode": false
    },
    {
      "id": "c9-s95",
      "chapterId": "c9",
      "blockId": "b12",
      "index": 95,
      "source": "參者一術一列。",
      "isCode": false
    },
    {
      "id": "c9-s96",
      "chapterId": "c9",
      "blockId": "b12",
      "index": 96,
      "source": "凡列中之物。",
      "isCode": false
    },
    {
      "id": "c9-s97",
      "chapterId": "c9",
      "blockId": "b12",
      "index": 97,
      "source": "一一施其術。",
      "isCode": false
    },
    {
      "id": "c9-s98",
      "chapterId": "c9",
      "blockId": "b12",
      "index": 98,
      "source": "另充於一列而得之也。",
      "isCode": false
    },
    {
      "id": "c9-s99",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 99,
      "source": "吾有一術。",
      "isCode": true
    },
    {
      "id": "c9-s100",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 100,
      "source": "名之曰「倍」。",
      "isCode": true
    },
    {
      "id": "c9-s101",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 101,
      "source": "欲行是術。",
      "isCode": true
    },
    {
      "id": "c9-s102",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 102,
      "source": "必先得一數。",
      "isCode": true
    },
    {
      "id": "c9-s103",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 103,
      "source": "名之曰「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s104",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 104,
      "source": "乃行是術曰。",
      "isCode": true
    },
    {
      "id": "c9-s105",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 105,
      "source": "\n\t乘「甲」以二。",
      "isCode": true
    },
    {
      "id": "c9-s106",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 106,
      "source": "乃得矣。",
      "isCode": true
    },
    {
      "id": "c9-s107",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 107,
      "source": "\n是謂「倍」之術也。",
      "isCode": true
    },
    {
      "id": "c9-s108",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 108,
      "source": "\n\n施「遍施」於「倍」於「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s109",
      "chapterId": "c9",
      "blockId": "b13",
      "index": 109,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s110",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 110,
      "source": "篩剔者。",
      "isCode": false
    },
    {
      "id": "c9-s111",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 111,
      "source": "參者一術一列。",
      "isCode": false
    },
    {
      "id": "c9-s112",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 112,
      "source": "凡列中之物。",
      "isCode": false
    },
    {
      "id": "c9-s113",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 113,
      "source": "先施其術。",
      "isCode": false
    },
    {
      "id": "c9-s114",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 114,
      "source": "得陽而留之。",
      "isCode": false
    },
    {
      "id": "c9-s115",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 115,
      "source": "得陰而棄之。",
      "isCode": false
    },
    {
      "id": "c9-s116",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 116,
      "source": "以為取捨。",
      "isCode": false
    },
    {
      "id": "c9-s117",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 117,
      "source": "乃聚得陽者為一列也。",
      "isCode": false
    },
    {
      "id": "c9-s118",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 118,
      "source": "前章有「篩剔」之例。",
      "isCode": false
    },
    {
      "id": "c9-s119",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 119,
      "source": "此其是也。",
      "isCode": false
    },
    {
      "id": "c9-s120",
      "chapterId": "c9",
      "blockId": "b14",
      "index": 120,
      "source": "故不贅以例也。",
      "isCode": false
    },
    {
      "id": "c9-s121",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 121,
      "source": "左右併者。",
      "isCode": false
    },
    {
      "id": "c9-s122",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 122,
      "source": "參者一術一元一列。",
      "isCode": false
    },
    {
      "id": "c9-s123",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 123,
      "source": "元者。",
      "isCode": false
    },
    {
      "id": "c9-s124",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 124,
      "source": "變數之類不預知者也。",
      "isCode": false
    },
    {
      "id": "c9-s125",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 125,
      "source": "蓋得其實而後定也。",
      "isCode": false
    },
    {
      "id": "c9-s126",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 126,
      "source": "其參術參二。",
      "isCode": false
    },
    {
      "id": "c9-s127",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 127,
      "source": "取列中一物及元。",
      "isCode": false
    },
    {
      "id": "c9-s128",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 128,
      "source": "施參術得新元。",
      "isCode": false
    },
    {
      "id": "c9-s129",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 129,
      "source": "次第為之。",
      "isCode": false
    },
    {
      "id": "c9-s130",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 130,
      "source": "縮列為元也。",
      "isCode": false
    },
    {
      "id": "c9-s131",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 131,
      "source": "左併者自列首順縮之。",
      "isCode": false
    },
    {
      "id": "c9-s132",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 132,
      "source": "參術之施先元後列。",
      "isCode": false
    },
    {
      "id": "c9-s133",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 133,
      "source": "右併者自列末逆縮之。",
      "isCode": false
    },
    {
      "id": "c9-s134",
      "chapterId": "c9",
      "blockId": "b15",
      "index": 134,
      "source": "參術之施先列後元。",
      "isCode": false
    },
    {
      "id": "c9-s135",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 135,
      "source": "吾有一術。",
      "isCode": true
    },
    {
      "id": "c9-s136",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 136,
      "source": "名之曰「合」。",
      "isCode": true
    },
    {
      "id": "c9-s137",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 137,
      "source": "欲行是術。",
      "isCode": true
    },
    {
      "id": "c9-s138",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 138,
      "source": "必先得二數。",
      "isCode": true
    },
    {
      "id": "c9-s139",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 139,
      "source": "名之曰「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s140",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 140,
      "source": "曰「乙」。",
      "isCode": true
    },
    {
      "id": "c9-s141",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 141,
      "source": "乃行是術曰。",
      "isCode": true
    },
    {
      "id": "c9-s142",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 142,
      "source": "\n\t加「甲」以「乙」。",
      "isCode": true
    },
    {
      "id": "c9-s143",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 143,
      "source": "乃得矣。",
      "isCode": true
    },
    {
      "id": "c9-s144",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 144,
      "source": "\n是謂「合」之術也。",
      "isCode": true
    },
    {
      "id": "c9-s145",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 145,
      "source": "\n\n施「左併」於「合」於零於「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s146",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 146,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s147",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 147,
      "source": "\n注曰。",
      "isCode": true
    },
    {
      "id": "c9-s148",
      "chapterId": "c9",
      "blockId": "b16",
      "index": 148,
      "source": "『所以得「甲」中數之總和也』",
      "isCode": true
    },
    {
      "id": "c9-s149",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 149,
      "source": "排序者。",
      "isCode": false
    },
    {
      "id": "c9-s150",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 150,
      "source": "參者一術一列。",
      "isCode": false
    },
    {
      "id": "c9-s151",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 151,
      "source": "易列之序。",
      "isCode": false
    },
    {
      "id": "c9-s152",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 152,
      "source": "令彼物自小而大者也。",
      "isCode": false
    },
    {
      "id": "c9-s153",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 153,
      "source": "其參術者。",
      "isCode": false
    },
    {
      "id": "c9-s154",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 154,
      "source": "所以定小大也。",
      "isCode": false
    },
    {
      "id": "c9-s155",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 155,
      "source": "蓋因數之小大易明。",
      "isCode": false
    },
    {
      "id": "c9-s156",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 156,
      "source": "然可排序者不唯數而已矣。",
      "isCode": false
    },
    {
      "id": "c9-s157",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 157,
      "source": "故小大之繩。",
      "isCode": false
    },
    {
      "id": "c9-s158",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 158,
      "source": "施者視其類而自定也。",
      "isCode": false
    },
    {
      "id": "c9-s159",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 159,
      "source": "其比二物也。",
      "isCode": false
    },
    {
      "id": "c9-s160",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 160,
      "source": "小之當得負。",
      "isCode": false
    },
    {
      "id": "c9-s161",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 161,
      "source": "大之當得正。",
      "isCode": false
    },
    {
      "id": "c9-s162",
      "chapterId": "c9",
      "blockId": "b17",
      "index": 162,
      "source": "等之當得零。",
      "isCode": false
    },
    {
      "id": "c9-s163",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 163,
      "source": "吾有一術。",
      "isCode": true
    },
    {
      "id": "c9-s164",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 164,
      "source": "名之曰「比」。",
      "isCode": true
    },
    {
      "id": "c9-s165",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 165,
      "source": "欲行是術。",
      "isCode": true
    },
    {
      "id": "c9-s166",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 166,
      "source": "必先得二數。",
      "isCode": true
    },
    {
      "id": "c9-s167",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 167,
      "source": "名之曰「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s168",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 168,
      "source": "曰「乙」。",
      "isCode": true
    },
    {
      "id": "c9-s169",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 169,
      "source": "乃行是術曰。",
      "isCode": true
    },
    {
      "id": "c9-s170",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 170,
      "source": "\n\t若「甲」小於「乙」者。",
      "isCode": true
    },
    {
      "id": "c9-s171",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 171,
      "source": "乃得負一也。",
      "isCode": true
    },
    {
      "id": "c9-s172",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 172,
      "source": "\n\t若「甲」大於「乙」者。",
      "isCode": true
    },
    {
      "id": "c9-s173",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 173,
      "source": "乃得一也。",
      "isCode": true
    },
    {
      "id": "c9-s174",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 174,
      "source": "\n\t乃得零。",
      "isCode": true
    },
    {
      "id": "c9-s175",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 175,
      "source": "\n\t注曰『或云「減甲以乙乃得矣」亦可』",
      "isCode": true
    },
    {
      "id": "c9-s176",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 176,
      "source": "\n是謂「比」之術也。",
      "isCode": true
    },
    {
      "id": "c9-s177",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 177,
      "source": "\n\n施「排序」於「比」於「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s178",
      "chapterId": "c9",
      "blockId": "b18",
      "index": 178,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s179",
      "chapterId": "c9",
      "blockId": "b19",
      "index": 179,
      "source": "倒序者。",
      "isCode": false
    },
    {
      "id": "c9-s180",
      "chapterId": "c9",
      "blockId": "b19",
      "index": 180,
      "source": "參者一列。",
      "isCode": false
    },
    {
      "id": "c9-s181",
      "chapterId": "c9",
      "blockId": "b19",
      "index": 181,
      "source": "唯逆其序耳。",
      "isCode": false
    },
    {
      "id": "c9-s182",
      "chapterId": "c9",
      "blockId": "b19",
      "index": 182,
      "source": "入者甲乙丙丁。",
      "isCode": false
    },
    {
      "id": "c9-s183",
      "chapterId": "c9",
      "blockId": "b19",
      "index": 183,
      "source": "出者丁丙乙甲也。",
      "isCode": false
    },
    {
      "id": "c9-s184",
      "chapterId": "c9",
      "blockId": "b20",
      "index": 184,
      "source": "施「倒序」於「甲」。",
      "isCode": true
    },
    {
      "id": "c9-s185",
      "chapterId": "c9",
      "blockId": "b20",
      "index": 185,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s186",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 186,
      "source": "索一者。",
      "isCode": false
    },
    {
      "id": "c9-s187",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 187,
      "source": "參者一列一元。",
      "isCode": false
    },
    {
      "id": "c9-s188",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 188,
      "source": "於列中尋其元也。",
      "isCode": false
    },
    {
      "id": "c9-s189",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 189,
      "source": "尋而得之者。",
      "isCode": false
    },
    {
      "id": "c9-s190",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 190,
      "source": "乃得其序數也。",
      "isCode": false
    },
    {
      "id": "c9-s191",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 191,
      "source": "首物等元者。",
      "isCode": false
    },
    {
      "id": "c9-s192",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 192,
      "source": "得一。",
      "isCode": false
    },
    {
      "id": "c9-s193",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 193,
      "source": "次物等元者。",
      "isCode": false
    },
    {
      "id": "c9-s194",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 194,
      "source": "得二。",
      "isCode": false
    },
    {
      "id": "c9-s195",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 195,
      "source": "不得者。",
      "isCode": false
    },
    {
      "id": "c9-s196",
      "chapterId": "c9",
      "blockId": "b21",
      "index": 196,
      "source": "乃得零也。",
      "isCode": false
    },
    {
      "id": "c9-s197",
      "chapterId": "c9",
      "blockId": "b22",
      "index": 197,
      "source": "施「索一」於「甲」於九。",
      "isCode": true
    },
    {
      "id": "c9-s198",
      "chapterId": "c9",
      "blockId": "b22",
      "index": 198,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s199",
      "chapterId": "c9",
      "blockId": "b22",
      "index": 199,
      "source": "\n施「索一」於「甲」於十五。",
      "isCode": true
    },
    {
      "id": "c9-s200",
      "chapterId": "c9",
      "blockId": "b22",
      "index": 200,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s201",
      "chapterId": "c9",
      "blockId": "b23",
      "index": 201,
      "source": "其餘諸書所羅者。",
      "isCode": false
    },
    {
      "id": "c9-s202",
      "chapterId": "c9",
      "blockId": "b23",
      "index": 202,
      "source": "亦多也。",
      "isCode": false
    },
    {
      "id": "c9-s203",
      "chapterId": "c9",
      "blockId": "b23",
      "index": 203,
      "source": "一一敘之者。",
      "isCode": false
    },
    {
      "id": "c9-s204",
      "chapterId": "c9",
      "blockId": "b23",
      "index": 204,
      "source": "則非此篇之本意也。",
      "isCode": false
    },
    {
      "id": "c9-s205",
      "chapterId": "c9",
      "blockId": "b23",
      "index": 205,
      "source": "唯「渾沌」「格物」「畫譜」三經。",
      "isCode": false
    },
    {
      "id": "c9-s206",
      "chapterId": "c9",
      "blockId": "b23",
      "index": 206,
      "source": "更見於後章。",
      "isCode": false
    },
    {
      "id": "c9-s207",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 207,
      "source": "或問曰。",
      "isCode": false
    },
    {
      "id": "c9-s208",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 208,
      "source": "此引他書之法吾明矣。",
      "isCode": false
    },
    {
      "id": "c9-s209",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 209,
      "source": "欲自為一書而引之。",
      "isCode": false
    },
    {
      "id": "c9-s210",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 210,
      "source": "可乎。",
      "isCode": false
    },
    {
      "id": "c9-s211",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 211,
      "source": "曰。",
      "isCode": false
    },
    {
      "id": "c9-s212",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 212,
      "source": "可也。",
      "isCode": false
    },
    {
      "id": "c9-s213",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 213,
      "source": "另立一程式。",
      "isCode": false
    },
    {
      "id": "c9-s214",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 214,
      "source": "命之以書名。",
      "isCode": false
    },
    {
      "id": "c9-s215",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 215,
      "source": "於其書中。",
      "isCode": false
    },
    {
      "id": "c9-s216",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 216,
      "source": "凡欲為他書所引之變數。",
      "isCode": false
    },
    {
      "id": "c9-s217",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 217,
      "source": "皆易「吾有」為「今有」。",
      "isCode": false
    },
    {
      "id": "c9-s218",
      "chapterId": "c9",
      "blockId": "b24",
      "index": 218,
      "source": "如是。",
      "isCode": false
    },
    {
      "id": "c9-s219",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 219,
      "source": "注曰『當名是程式曰「勾股算經』」。",
      "isCode": true
    },
    {
      "id": "c9-s220",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 220,
      "source": "\n\n吾嘗觀「算經」之書。",
      "isCode": true
    },
    {
      "id": "c9-s221",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 221,
      "source": "方悟「平方根」之義。",
      "isCode": true
    },
    {
      "id": "c9-s222",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 222,
      "source": "\n注曰『書書相引也』",
      "isCode": true
    },
    {
      "id": "c9-s223",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 223,
      "source": "\n\n今有一術。",
      "isCode": true
    },
    {
      "id": "c9-s224",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 224,
      "source": "名之曰「勾股求弦」。",
      "isCode": true
    },
    {
      "id": "c9-s225",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 225,
      "source": "欲行是術。",
      "isCode": true
    },
    {
      "id": "c9-s226",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 226,
      "source": "必先得二數。",
      "isCode": true
    },
    {
      "id": "c9-s227",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 227,
      "source": "曰「勾」。",
      "isCode": true
    },
    {
      "id": "c9-s228",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 228,
      "source": "曰「股」。",
      "isCode": true
    },
    {
      "id": "c9-s229",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 229,
      "source": "乃行是術曰。",
      "isCode": true
    },
    {
      "id": "c9-s230",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 230,
      "source": "\n\t乘「勾」以「勾」。",
      "isCode": true
    },
    {
      "id": "c9-s231",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 231,
      "source": "名之曰「勾方」。",
      "isCode": true
    },
    {
      "id": "c9-s232",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 232,
      "source": "\n\t乘「股」以「股」。",
      "isCode": true
    },
    {
      "id": "c9-s233",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 233,
      "source": "加其於「勾方」。",
      "isCode": true
    },
    {
      "id": "c9-s234",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 234,
      "source": "取一以施「平方根」。",
      "isCode": true
    },
    {
      "id": "c9-s235",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 235,
      "source": "乃得矣。",
      "isCode": true
    },
    {
      "id": "c9-s236",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 236,
      "source": "\n是謂「勾股求弦」之術也。",
      "isCode": true
    },
    {
      "id": "c9-s237",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 237,
      "source": "\n\n今有一術。",
      "isCode": true
    },
    {
      "id": "c9-s238",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 238,
      "source": "名之曰「勾弦求股」。",
      "isCode": true
    },
    {
      "id": "c9-s239",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 239,
      "source": "欲行是術。",
      "isCode": true
    },
    {
      "id": "c9-s240",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 240,
      "source": "必先得二數。",
      "isCode": true
    },
    {
      "id": "c9-s241",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 241,
      "source": "曰「勾」。",
      "isCode": true
    },
    {
      "id": "c9-s242",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 242,
      "source": "曰「弦」。",
      "isCode": true
    },
    {
      "id": "c9-s243",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 243,
      "source": "乃行是術曰。",
      "isCode": true
    },
    {
      "id": "c9-s244",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 244,
      "source": "\n\t乘「勾」以「勾」。",
      "isCode": true
    },
    {
      "id": "c9-s245",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 245,
      "source": "名之曰「勾方」。",
      "isCode": true
    },
    {
      "id": "c9-s246",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 246,
      "source": "\n\t乘「弦」以「弦」。",
      "isCode": true
    },
    {
      "id": "c9-s247",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 247,
      "source": "減其以「勾方」。",
      "isCode": true
    },
    {
      "id": "c9-s248",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 248,
      "source": "取一以施「平方根」。",
      "isCode": true
    },
    {
      "id": "c9-s249",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 249,
      "source": "乃得矣。",
      "isCode": true
    },
    {
      "id": "c9-s250",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 250,
      "source": "\n是謂「勾弦求股」之術也。",
      "isCode": true
    },
    {
      "id": "c9-s251",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 251,
      "source": "\n\n今有三數。",
      "isCode": true
    },
    {
      "id": "c9-s252",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 252,
      "source": "曰三。",
      "isCode": true
    },
    {
      "id": "c9-s253",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 253,
      "source": "曰四。",
      "isCode": true
    },
    {
      "id": "c9-s254",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 254,
      "source": "曰五。",
      "isCode": true
    },
    {
      "id": "c9-s255",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 255,
      "source": "名之曰「勾例」",
      "isCode": true
    },
    {
      "id": "c9-s256",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 256,
      "source": "曰「股例」",
      "isCode": true
    },
    {
      "id": "c9-s257",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 257,
      "source": "曰「弦例」。",
      "isCode": true
    },
    {
      "id": "c9-s258",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 258,
      "source": "\n\n吾有一數。",
      "isCode": true
    },
    {
      "id": "c9-s259",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 259,
      "source": "曰六。",
      "isCode": true
    },
    {
      "id": "c9-s260",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 260,
      "source": "名之曰「唯吾知之」。",
      "isCode": true
    },
    {
      "id": "c9-s261",
      "chapterId": "c9",
      "blockId": "b25",
      "index": 261,
      "source": "注曰『凡以「吾有」言變數者。他書不得引之』",
      "isCode": true
    },
    {
      "id": "c9-s262",
      "chapterId": "c9",
      "blockId": "b26",
      "index": 262,
      "source": "是書既成。",
      "isCode": false
    },
    {
      "id": "c9-s263",
      "chapterId": "c9",
      "blockId": "b26",
      "index": 263,
      "source": "乃另作一程式。",
      "isCode": false
    },
    {
      "id": "c9-s264",
      "chapterId": "c9",
      "blockId": "b26",
      "index": 264,
      "source": "引而驗之。",
      "isCode": false
    },
    {
      "id": "c9-s265",
      "chapterId": "c9",
      "blockId": "b27",
      "index": 265,
      "source": "吾嘗觀『勾股算經』之書。",
      "isCode": true
    },
    {
      "id": "c9-s266",
      "chapterId": "c9",
      "blockId": "b27",
      "index": 266,
      "source": "方悟「勾股求弦」「勾弦求股」「勾例」「股例」之義。",
      "isCode": true
    },
    {
      "id": "c9-s267",
      "chapterId": "c9",
      "blockId": "b27",
      "index": 267,
      "source": "\n\n施「勾股求弦」於「勾例」「股例」。",
      "isCode": true
    },
    {
      "id": "c9-s268",
      "chapterId": "c9",
      "blockId": "b27",
      "index": 268,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s269",
      "chapterId": "c9",
      "blockId": "b27",
      "index": 269,
      "source": "\n施「勾弦求股」於五。",
      "isCode": true
    },
    {
      "id": "c9-s270",
      "chapterId": "c9",
      "blockId": "b27",
      "index": 270,
      "source": "於十三。",
      "isCode": true
    },
    {
      "id": "c9-s271",
      "chapterId": "c9",
      "blockId": "b27",
      "index": 271,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c9-s272",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 272,
      "source": "著引二道。",
      "isCode": false
    },
    {
      "id": "c9-s273",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 273,
      "source": "至是略備矣。",
      "isCode": false
    },
    {
      "id": "c9-s274",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 274,
      "source": "程式既長。",
      "isCode": false
    },
    {
      "id": "c9-s275",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 275,
      "source": "使盡入一文。",
      "isCode": false
    },
    {
      "id": "c9-s276",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 276,
      "source": "輒如亂粥也。",
      "isCode": false
    },
    {
      "id": "c9-s277",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 277,
      "source": "故立大事者。",
      "isCode": false
    },
    {
      "id": "c9-s278",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 278,
      "source": "多擘其文。",
      "isCode": false
    },
    {
      "id": "c9-s279",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 279,
      "source": "分作數書。",
      "isCode": false
    },
    {
      "id": "c9-s280",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 280,
      "source": "而更相引用。",
      "isCode": false
    },
    {
      "id": "c9-s281",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 281,
      "source": "脈絡既明。",
      "isCode": false
    },
    {
      "id": "c9-s282",
      "chapterId": "c9",
      "blockId": "b28",
      "index": 282,
      "source": "亦大裨益於檢索也。",
      "isCode": false
    }
  ]
}