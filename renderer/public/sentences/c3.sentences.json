    },
    {
      "id": "c3-s217",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 217,
      "source": "名之曰「許狐和」。",
      "isCode": true
    },
    {
      "id": "c3-s218",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 218,
      "source": "\n乘「許狐和」以十。",
      "isCode": true
    },
    {
      "id": "c3-s219",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 219,
      "source": "名之曰「十倍許狐和」。",
