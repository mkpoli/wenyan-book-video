    },
    {
      "id": "c3-s226",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 226,
      "source": "除其以八。",
      "isCode": true
    },
    {
      "id": "c3-s227",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 227,
      "source": "名之曰「九尾狐」。",
      "isCode": true
    },
    {
      "id": "c3-s228",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 228,
      "source": "\n減「許狐和」以「九尾狐」。",
      "isCode": true
    },
    {
      "id": "c3-s229",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 229,
      "source": "名之曰「許德拉」。",
      "isCode": true
    },
    {
      "id": "c3-s230",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 230,
      "source": "\n夫「許德拉」夫「九尾狐」夫「刑天」夫「兩頭蛇」。",
      "isCode": true
    },
    {
      "id": "c3-s231",
      "chapterId": "c3",
      "blockId": "b26",
      "index": 231,
      "source": "書之。",
      "isCode": true
    },
    {
      "id": "c3-s232",
      "chapterId": "c3",
      "blockId": "b27",
      "index": 232,
      "source": "乃得",
      "isCode": false
    },
    {
      "id": "c3-s233",
      "chapterId": "c3",
      "blockId": "b28",
      "index": 233,
      "source": "一十二 三十四 一十六 九十九",
      "isCode": true
    },
    {
      "id": "c3-s234",
      "chapterId": "c3",
      "blockId": "b29",
      "index": 234,
      "source": "其解法蓋與雉兔同籠相類。",
