    },
    {
      "id": "c3-s235",
      "chapterId": "c3",
      "blockId": "b29",
      "index": 235,
      "source": "算術之道。",
      "isCode": false
    },
    {
      "id": "c3-s236",
      "chapterId": "c3",
      "blockId": "b29",
      "index": 236,
      "source": "至是少備矣。",
      "isCode": false
    },
    {
      "id": "c3-s237",
      "chapterId": "c3",
      "blockId": "b29",
      "index": 237,
      "source": "更有圓周密率。",
