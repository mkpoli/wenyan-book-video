{
  "chapterId": "c4",
  "number": 4,
  "title": "決策第四",
  "sentences": [
    {
      "id": "c4-s1",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 1,
      "source": "決策者。",
      "isCode": false
    },
    {
      "id": "c4-s2",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 2,
      "source": "萬智之始。",
      "isCode": false
    },
    {
      "id": "c4-s3",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 3,
      "source": "何耶。",
      "isCode": false
    },
    {
      "id": "c4-s4",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 4,
      "source": "曰察而後動。",
      "isCode": false
    },
    {
      "id": "c4-s5",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 5,
      "source": "魚之相吞。",
      "isCode": false
    },
    {
      "id": "c4-s6",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 6,
      "source": "必先度彼之大小。",
      "isCode": false
    },
    {
      "id": "c4-s7",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 7,
      "source": "犬之嚙人。",
      "isCode": false
    },
    {
      "id": "c4-s8",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 8,
      "source": "必先觀主之喜惡。",
      "isCode": false
    },
    {
      "id": "c4-s9",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 9,
      "source": "此禽獸之決策也。",
      "isCode": false
    },
    {
      "id": "c4-s10",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 10,
      "source": "雨而蓑。",
      "isCode": false
    },
    {
      "id": "c4-s11",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 11,
      "source": "寒而衣。",
      "isCode": false
    },
    {
      "id": "c4-s12",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 12,
      "source": "價善乃沽。",
      "isCode": false
    },
    {
      "id": "c4-s13",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 13,
      "source": "人善乃欺。",
      "isCode": false
    },
    {
      "id": "c4-s14",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 14,
      "source": "匹夫之決策也。",
      "isCode": false
    },
    {
      "id": "c4-s15",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 15,
      "source": "陟罰臧否。",
      "isCode": false
    },
    {
      "id": "c4-s16",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 16,
      "source": "伐謀伐交。",
      "isCode": false
    },
    {
      "id": "c4-s17",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 17,
      "source": "卿相之決策也。",
      "isCode": false
    },
    {
      "id": "c4-s18",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 18,
      "source": "其所以智於向者。",
      "isCode": false
    },
    {
      "id": "c4-s19",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 19,
      "source": "唯決策有難易之別耳。",
      "isCode": false
    },
    {
      "id": "c4-s20",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 20,
      "source": "故程式之能於決策者。",
      "isCode": false
    },
    {
      "id": "c4-s21",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 21,
      "source": "智比於人。",
      "isCode": false
    },
    {
      "id": "c4-s22",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 22,
      "source": "不能者。",
      "isCode": false
    },
    {
      "id": "c4-s23",
      "chapterId": "c4",
      "blockId": "b1",
      "index": 23,
      "source": "止一算盤耳。",
      "isCode": false
    },
    {
      "id": "c4-s24",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 24,
      "source": "其決步驟有三。",
      "isCode": false
    },
    {
      "id": "c4-s25",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 25,
      "source": "一曰比之。",
      "isCode": false
    },
    {
      "id": "c4-s26",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 26,
      "source": "二曰決之。",
      "isCode": false
    },
    {
      "id": "c4-s27",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 27,
      "source": "三曰為之。",
      "isCode": false
    },
    {
      "id": "c4-s28",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 28,
      "source": "今以馬太效應為例。",
      "isCode": false
    },
    {
      "id": "c4-s29",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 29,
      "source": "書之如是。",
      "isCode": false
    },
    {
      "id": "c4-s30",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 30,
      "source": "注。",
      "isCode": false
    },
    {
      "id": "c4-s31",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 31,
      "source": "馬太效應者。",
      "isCode": false
    },
    {
      "id": "c4-s32",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 32,
      "source": "西人典故也。",
      "isCode": false
    },
    {
      "id": "c4-s33",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 33,
      "source": "曰。",
      "isCode": false
    },
    {
      "id": "c4-s34",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 34,
      "source": "盈者愈予之。",
      "isCode": false
    },
    {
      "id": "c4-s35",
      "chapterId": "c4",
      "blockId": "b2",
      "index": 35,
      "source": "乏者更奪之。",
      "isCode": false
    },
    {
      "id": "c4-s36",
      "chapterId": "c4",
      "blockId": "b3",
      "index": 36,
      "source": "吾有一數。曰四十九。名之曰「貯」。\n若「貯」大於五十者。\n\t加「貯」以一。昔之「貯」者。今其是矣。\n若非。\n\t減「貯」以一。昔之「貯」者。今其是矣。\n也。",
      "isCode": true
    },
    {
      "id": "c4-s37",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 37,
      "source": "`大於`者。",
      "isCode": false
    },
    {
      "id": "c4-s38",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 38,
      "source": "比之也。",
      "isCode": false
    },
    {
      "id": "c4-s39",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 39,
      "source": "`若`。",
      "isCode": false
    },
    {
      "id": "c4-s40",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 40,
      "source": "`若非`者。",
      "isCode": false
    },
    {
      "id": "c4-s41",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 41,
      "source": "決之也。",
      "isCode": false
    },
    {
      "id": "c4-s42",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 42,
      "source": "`加「貯」以一`云云者。",
      "isCode": false
    },
    {
      "id": "c4-s43",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 43,
      "source": "為之也。",
      "isCode": false
    },
    {
      "id": "c4-s44",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 44,
      "source": "比之得其真者。",
      "isCode": false
    },
    {
      "id": "c4-s45",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 45,
      "source": "必為`若`云云之次句。",
      "isCode": false
    },
    {
      "id": "c4-s46",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 46,
      "source": "必不為`若非`之次句。",
      "isCode": false
    },
    {
      "id": "c4-s47",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 47,
      "source": "否。",
      "isCode": false
    },
    {
      "id": "c4-s48",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 48,
      "source": "則反之。",
      "isCode": false
    },
    {
      "id": "c4-s49",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 49,
      "source": "今以四十九不大於五十。",
      "isCode": false
    },
    {
      "id": "c4-s50",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 50,
      "source": "遂行`若非`之次句。",
      "isCode": false
    },
    {
      "id": "c4-s51",
      "chapterId": "c4",
      "blockId": "b4",
      "index": 51,
      "source": "`「貯」`竟四十八。",
      "isCode": false
    },
    {
      "id": "c4-s52",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 52,
      "source": "或問曰。",
      "isCode": false
    },
    {
      "id": "c4-s53",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 53,
      "source": "末之`也`字者。",
      "isCode": false
    },
    {
      "id": "c4-s54",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 54,
      "source": "語助耶。",
      "isCode": false
    },
    {
      "id": "c4-s55",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 55,
      "source": "無之可乎。",
      "isCode": false
    },
    {
      "id": "c4-s56",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 56,
      "source": "曰。",
      "isCode": false
    },
    {
      "id": "c4-s57",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 57,
      "source": "不可也。",
      "isCode": false
    },
    {
      "id": "c4-s58",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 58,
      "source": "所以別歧義。",
      "isCode": false
    },
    {
      "id": "c4-s59",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 59,
      "source": "示一句之終止也。",
      "isCode": false
    },
    {
      "id": "c4-s60",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 60,
      "source": "蓋編程之理或有極繁者。",
      "isCode": false
    },
    {
      "id": "c4-s61",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 61,
      "source": "或相嵌。",
      "isCode": false
    },
    {
      "id": "c4-s62",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 62,
      "source": "或相銜。",
      "isCode": false
    },
    {
      "id": "c4-s63",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 63,
      "source": "亡是不能斷。",
      "isCode": false
    },
    {
      "id": "c4-s64",
      "chapterId": "c4",
      "blockId": "b5",
      "index": 64,
      "source": "用`云云`二字亦可。",
      "isCode": false
    },
    {
      "id": "c4-s65",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 65,
      "source": "又問曰。",
      "isCode": false
    },
    {
      "id": "c4-s66",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 66,
      "source": "或雖非亦於我無與者。",
      "isCode": false
    },
    {
      "id": "c4-s67",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 67,
      "source": "省`若非`之句。",
      "isCode": false
    },
    {
      "id": "c4-s68",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 68,
      "source": "可乎。",
      "isCode": false
    },
    {
      "id": "c4-s69",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 69,
      "source": "曰。",
      "isCode": false
    },
    {
      "id": "c4-s70",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 70,
      "source": "可也。",
      "isCode": false
    },
    {
      "id": "c4-s71",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 71,
      "source": "例曰今有十斗之器。",
      "isCode": false
    },
    {
      "id": "c4-s72",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 72,
      "source": "主人有酒若干。",
      "isCode": false
    },
    {
      "id": "c4-s73",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 73,
      "source": "傾諸此器。",
      "isCode": false
    },
    {
      "id": "c4-s74",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 74,
      "source": "十斗以上者皆滿盈。",
      "isCode": false
    },
    {
      "id": "c4-s75",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 75,
      "source": "問器有酒竟幾何。",
      "isCode": false
    },
    {
      "id": "c4-s76",
      "chapterId": "c4",
      "blockId": "b6",
      "index": 76,
      "source": "解曰。",
      "isCode": false
    },
    {
      "id": "c4-s77",
      "chapterId": "c4",
      "blockId": "b7",
      "index": 77,
      "source": "吾有二數。曰十。曰九。名之曰「器量」曰「酒量」。\n若「酒量」大於「器量」者。\n\t昔之「酒量」者。今「器量」是矣。\n云云。\n夫「酒量」。書之。",
      "isCode": true
    },
    {
      "id": "c4-s78",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 78,
      "source": "比數之法有六。",
      "isCode": false
    },
    {
      "id": "c4-s79",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 79,
      "source": "曰等於。",
      "isCode": false
    },
    {
      "id": "c4-s80",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 80,
      "source": "曰不等於。",
      "isCode": false
    },
    {
      "id": "c4-s81",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 81,
      "source": "曰大於。",
      "isCode": false
    },
    {
      "id": "c4-s82",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 82,
      "source": "曰小於。",
      "isCode": false
    },
    {
      "id": "c4-s83",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 83,
      "source": "曰不大於。",
      "isCode": false
    },
    {
      "id": "c4-s84",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 84,
      "source": "曰不小於。",
      "isCode": false
    },
    {
      "id": "c4-s85",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 85,
      "source": "比他物之法有二。",
      "isCode": false
    },
    {
      "id": "c4-s86",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 86,
      "source": "曰等於。",
      "isCode": false
    },
    {
      "id": "c4-s87",
      "chapterId": "c4",
      "blockId": "b8",
      "index": 87,
      "source": "曰不等於。",
      "isCode": false
    },
    {
      "id": "c4-s88",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 88,
      "source": "或問曰。",
      "isCode": false
    },
    {
      "id": "c4-s89",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 89,
      "source": "事有千端。",
      "isCode": false
    },
    {
      "id": "c4-s90",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 90,
      "source": "非一策能定者。",
      "isCode": false
    },
    {
      "id": "c4-s91",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 91,
      "source": "奈何。",
      "isCode": false
    },
    {
      "id": "c4-s92",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 92,
      "source": "曰。",
      "isCode": false
    },
    {
      "id": "c4-s93",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 93,
      "source": "當用`或若`。",
      "isCode": false
    },
    {
      "id": "c4-s94",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 94,
      "source": "`或若`者。",
      "isCode": false
    },
    {
      "id": "c4-s95",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 95,
      "source": "一`若`之後。",
      "isCode": false
    },
    {
      "id": "c4-s96",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 96,
      "source": "連綿用之。",
      "isCode": false
    },
    {
      "id": "c4-s97",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 97,
      "source": "因序而比。",
      "isCode": false
    },
    {
      "id": "c4-s98",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 98,
      "source": "遇真輒為之。",
      "isCode": false
    },
    {
      "id": "c4-s99",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 99,
      "source": "為之乃止。",
      "isCode": false
    },
    {
      "id": "c4-s100",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 100,
      "source": "不更比也。",
      "isCode": false
    },
    {
      "id": "c4-s101",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 101,
      "source": "如皆不是者。",
      "isCode": false
    },
    {
      "id": "c4-s102",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 102,
      "source": "厥行最末`若非`之句。",
      "isCode": false
    },
    {
      "id": "c4-s103",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 103,
      "source": "老氏有天地不仁。",
      "isCode": false
    },
    {
      "id": "c4-s104",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 104,
      "source": "以萬物為芻狗之句。",
      "isCode": false
    },
    {
      "id": "c4-s105",
      "chapterId": "c4",
      "blockId": "b9",
      "index": 105,
      "source": "今以`或若`擬天策如是。",
      "isCode": false
    },
    {
      "id": "c4-s106",
      "chapterId": "c4",
      "blockId": "b10",
      "index": 106,
      "source": "有言「「人」」。名之曰「物」。\n\n若「物」等於「「禽獸」」者。\n\t吾有一言。曰「「爾芻狗也。」」書之。\n或若「物」等於「「草木」」者。\n\t吾有一言。曰「「爾亦芻狗也。」」書之。\n或若「物」等於「「人」」者。\n\t吾有一言。曰「「爾雖人。於我實芻狗也。」」書之。\n或若「物」等於「「芻狗」」者。\n\t吾有一言。曰「「更不待言。」」書之。\n若非。\n\t吾有一言。曰「「吾不知爾何物。然爾之為芻狗明也。」」書之。\n云云。\n\n昔之「物」者。今「「芻狗」」是矣。",
      "isCode": true
    },
    {
      "id": "c4-s107",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 107,
      "source": "爻者。",
      "isCode": false
    },
    {
      "id": "c4-s108",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 108,
      "source": "陰陽也。",
      "isCode": false
    },
    {
      "id": "c4-s109",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 109,
      "source": "是非也。",
      "isCode": false
    },
    {
      "id": "c4-s110",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 110,
      "source": "有無也。",
      "isCode": false
    },
    {
      "id": "c4-s111",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 111,
      "source": "臧否也。",
      "isCode": false
    },
    {
      "id": "c4-s112",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 112,
      "source": "見於變數之章。",
      "isCode": false
    },
    {
      "id": "c4-s113",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 113,
      "source": "今用諸決策。",
      "isCode": false
    },
    {
      "id": "c4-s114",
      "chapterId": "c4",
      "blockId": "b11",
      "index": 114,
      "source": "不亦宜乎。",
      "isCode": false
    },
    {
      "id": "c4-s115",
      "chapterId": "c4",
      "blockId": "b12",
      "index": 115,
      "source": "吾有一爻。曰陽。名之曰「甲」。\n若「甲」者。\n\t吾有一言。曰「「陽者。歲之主也。」」書之。\n若非。\n\t吾有一言。曰「「陰者。陽之助也。」」書之。\n也。",
      "isCode": true
    },
    {
      "id": "c4-s116",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 116,
      "source": "易曰。",
      "isCode": false
    },
    {
      "id": "c4-s117",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 117,
      "source": "爻者。",
      "isCode": false
    },
    {
      "id": "c4-s118",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 118,
      "source": "言乎變者也。",
      "isCode": false
    },
    {
      "id": "c4-s119",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 119,
      "source": "今爻之變。",
      "isCode": false
    },
    {
      "id": "c4-s120",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 120,
      "source": "其日用者有三。",
      "isCode": false
    },
    {
      "id": "c4-s121",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 121,
      "source": "曰與。",
      "isCode": false
    },
    {
      "id": "c4-s122",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 122,
      "source": "曰或。",
      "isCode": false
    },
    {
      "id": "c4-s123",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 123,
      "source": "曰非。",
      "isCode": false
    },
    {
      "id": "c4-s124",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 124,
      "source": "二爻皆陽謂之與。",
      "isCode": false
    },
    {
      "id": "c4-s125",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 125,
      "source": "二爻有陽謂之或。",
      "isCode": false
    },
    {
      "id": "c4-s126",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 126,
      "source": "陰者陽陽者陰謂之非。",
      "isCode": false
    },
    {
      "id": "c4-s127",
      "chapterId": "c4",
      "blockId": "b13",
      "index": 127,
      "source": "各書如是。",
      "isCode": false
    },
    {
      "id": "c4-s128",
      "chapterId": "c4",
      "blockId": "b14",
      "index": 128,
      "source": "夫「甲」「乙」中無陰乎。名之曰「丙」。\n夫「甲」「乙」中有陽乎。名之曰「丙」。\n變「甲」。名之曰「丙」。",
      "isCode": true
    },
    {
      "id": "c4-s129",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 129,
      "source": "更有與非。",
      "isCode": false
    },
    {
      "id": "c4-s130",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 130,
      "source": "或非。",
      "isCode": false
    },
    {
      "id": "c4-s131",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 131,
      "source": "異或諸術。",
      "isCode": false
    },
    {
      "id": "c4-s132",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 132,
      "source": "皆得諸向三者。",
      "isCode": false
    },
    {
      "id": "c4-s133",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 133,
      "source": "與非者。",
      "isCode": false
    },
    {
      "id": "c4-s134",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 134,
      "source": "二爻有陰。",
      "isCode": false
    },
    {
      "id": "c4-s135",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 135,
      "source": "或非者。",
      "isCode": false
    },
    {
      "id": "c4-s136",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 136,
      "source": "二爻皆陰。",
      "isCode": false
    },
    {
      "id": "c4-s137",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 137,
      "source": "異或者。",
      "isCode": false
    },
    {
      "id": "c4-s138",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 138,
      "source": "一陰一陽。",
      "isCode": false
    },
    {
      "id": "c4-s139",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 139,
      "source": "而六者亦皆可得諸與非。",
      "isCode": false
    },
    {
      "id": "c4-s140",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 140,
      "source": "故曰。",
      "isCode": false
    },
    {
      "id": "c4-s141",
      "chapterId": "c4",
      "blockId": "b15",
      "index": 141,
      "source": "與非生萬物。",
      "isCode": false
    },
    {
      "id": "c4-s142",
      "chapterId": "c4",
      "blockId": "b16",
      "index": 142,
      "source": "又有`若其然者`。",
      "isCode": false
    },
    {
      "id": "c4-s143",
      "chapterId": "c4",
      "blockId": "b16",
      "index": 143,
      "source": "`若其不然`者。",
      "isCode": false
    },
    {
      "id": "c4-s144",
      "chapterId": "c4",
      "blockId": "b16",
      "index": 144,
      "source": "以資爻策。",
      "isCode": false
    },
    {
      "id": "c4-s145",
      "chapterId": "c4",
      "blockId": "b16",
      "index": 145,
      "source": "故是二句同。",
      "isCode": false
    },
    {
      "id": "c4-s146",
      "chapterId": "c4",
      "blockId": "b17",
      "index": 146,
      "source": "夫「甲」「乙」中無陰乎。名之曰「丙」。若「丙」者。吾有一言。曰「「古之人誠不我欺。」」書之也。\n夫「甲」「乙」中無陰乎。若其然者。吾有一言。曰「「古之人誠不我欺。」」書之也。",
      "isCode": true
    },
    {
      "id": "c4-s147",
      "chapterId": "c4",
      "blockId": "b18",
      "index": 147,
      "source": "是二句亦同。",
      "isCode": false
    },
    {
      "id": "c4-s148",
      "chapterId": "c4",
      "blockId": "b19",
      "index": 148,
      "source": "夫「甲」「乙」中有陽乎。名之曰「丙」。變「丙」。名之曰「丁」。若「丁」者。吾有一言。曰「「古之人誠不我欺。」」書之也。\n夫「甲」「乙」中有陽乎。若其不然者。吾有一言。曰「「古之人誠不我欺。」」書之也。",
      "isCode": true
    },
    {
      "id": "c4-s149",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 149,
      "source": "孔門十哲。",
      "isCode": false
    },
    {
      "id": "c4-s150",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 150,
      "source": "顏回。",
      "isCode": false
    },
    {
      "id": "c4-s151",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 151,
      "source": "閔損。",
      "isCode": false
    },
    {
      "id": "c4-s152",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 152,
      "source": "冉伯牛。",
      "isCode": false
    },
    {
      "id": "c4-s153",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 153,
      "source": "仲弓。",
      "isCode": false
    },
    {
      "id": "c4-s154",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 154,
      "source": "宰予。",
      "isCode": false
    },
    {
      "id": "c4-s155",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 155,
      "source": "子貢。",
      "isCode": false
    },
    {
      "id": "c4-s156",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 156,
      "source": "冉求。",
      "isCode": false
    },
    {
      "id": "c4-s157",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 157,
      "source": "子路。",
      "isCode": false
    },
    {
      "id": "c4-s158",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 158,
      "source": "子游。",
      "isCode": false
    },
    {
      "id": "c4-s159",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 159,
      "source": "子夏。",
      "isCode": false
    },
    {
      "id": "c4-s160",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 160,
      "source": "並稱先賢。",
      "isCode": false
    },
    {
      "id": "c4-s161",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 161,
      "source": "而風采各異。",
      "isCode": false
    },
    {
      "id": "c4-s162",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 162,
      "source": "今四問之內。",
      "isCode": false
    },
    {
      "id": "c4-s163",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 163,
      "source": "可知其誰。",
      "isCode": false
    },
    {
      "id": "c4-s164",
      "chapterId": "c4",
      "blockId": "b20",
      "index": 164,
      "source": "其法如是。",
      "isCode": false
    },
    {
      "id": "c4-s165",
      "chapterId": "c4",
      "blockId": "b21",
      "index": 165,
      "source": "夫「魯人耶」。若其然者。\n\t夫「德行科耶」。若其然者。\n\t\t夫「未仕耶」。若其然者。\n\t\t\t夫「蚤死耶」。若其然者。\n\t\t\t\t吾有一言。曰「「賢哉。回也。人不堪其憂。回也不改其樂。」」書之。\n\t\t\t若非。\n\t\t\t\t吾有一言。曰「「孝哉。閔子騫。人不間於其父母昆弟之間。」」書之也。\n\t\t若非。\n\t\t\t夫「病厲耶」。若其然者。\n\t\t\t\t吾有一言。曰「「亡之。命也夫。斯人也。而有斯疾也。」」書之。\n\t\t\t若非。\n\t\t\t\t吾有一言。曰「「雍也。可使南面。」」書之也。\n\t\t云云。\n\t若非。\n\t\t夫「政事科耶」。若其然者。\n\t\t\t夫「戰死耶」。若其然者。\n\t\t\t\t吾有一言。曰「「若由也。不得其死然。」」書之。\n\t\t\t若非。\n\t\t\t\t吾有一言。曰「「求。無乃爾是過與。」」書之也。\n\t\t若非。\n\t\t\t吾有一言。曰「「朽木不可雕也。糞土之牆不可杇也。於予與何誅。」」書之也。\n\t云云。\n若非。\n\t夫「複姓耶」。若其然者。\n\t\t吾有一言。曰「「賜也。始可與言詩已矣。告諸往而知來者。」」書之。\n\t若非。\n\t\t夫「衛人耶」。若其然者。\n\t\t\t吾有一言。曰「「起予者。商也。始可與言詩已矣。」」書之。\n\t\t若非。\n\t\t\t吾有一言。曰「「二三子。偃之言是也。」」書之也。\n\t云云。\n云云。",
      "isCode": true
    },
    {
      "id": "c4-s166",
      "chapterId": "c4",
      "blockId": "b22",
      "index": 166,
      "source": "仲由字子路。",
      "isCode": false
    },
    {
      "id": "c4-s167",
      "chapterId": "c4",
      "blockId": "b22",
      "index": 167,
      "source": "魯人。",
      "isCode": false
    },
    {
      "id": "c4-s168",
      "chapterId": "c4",
      "blockId": "b22",
      "index": 168,
      "source": "政事科。",
      "isCode": false
    },
    {
      "id": "c4-s169",
      "chapterId": "c4",
      "blockId": "b22",
      "index": 169,
      "source": "仕於魯衛。",
      "isCode": false
    },
    {
      "id": "c4-s170",
      "chapterId": "c4",
      "blockId": "b22",
      "index": 170,
      "source": "戰死。",
      "isCode": false
    },
    {
      "id": "c4-s171",
      "chapterId": "c4",
      "blockId": "b22",
      "index": 171,
      "source": "今一試之。",
      "isCode": false
    },
    {
      "id": "c4-s172",
      "chapterId": "c4",
      "blockId": "b23",
      "index": 172,
      "source": "吾有九爻。曰陽。曰陰。曰陰。曰陽。曰陰。曰陰。曰陰。曰陰。曰陽。\n名之曰「魯人耶」曰「衛人耶」曰「德行科耶」曰「政事科耶」\n曰「複姓耶」曰「未仕耶」曰「蚤死耶」曰「病厲耶」曰「戰死耶」",
      "isCode": true
    },
    {
      "id": "c4-s173",
      "chapterId": "c4",
      "blockId": "b24",
      "index": 173,
      "source": "乃得",
      "isCode": false
    },
    {
      "id": "c4-s174",
      "chapterId": "c4",
      "blockId": "b25",
      "index": 174,
      "source": "若由也。不得其死然。",
      "isCode": true
    },
    {
      "id": "c4-s175",
      "chapterId": "c4",
      "blockId": "b26",
      "index": 175,
      "source": "似此決策連環相嵌者。",
      "isCode": false
    },
    {
      "id": "c4-s176",
      "chapterId": "c4",
      "blockId": "b26",
      "index": 176,
      "source": "或取其形謂之決策樹。",
      "isCode": false
    },
    {
      "id": "c4-s177",
      "chapterId": "c4",
      "blockId": "b26",
      "index": 177,
      "source": "編程決策之道。",
      "isCode": false
    },
    {
      "id": "c4-s178",
      "chapterId": "c4",
      "blockId": "b26",
      "index": 178,
      "source": "至是略備矣。",
      "isCode": false
    }
  ]
}